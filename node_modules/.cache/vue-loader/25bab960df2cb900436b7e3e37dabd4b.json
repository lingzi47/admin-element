\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"序号\" align=\"center\"> </el-table-column>\r\n\r\n      <el-table-column label=\"活动\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span\r\n            >普通用户商城消费单笔满{{ scope.row.money }}元赠送黑卡会员{{\r\n              scope.row.days\r\n            }}日</span\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"time\" label=\"活动时间\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"open(scope.row)\"\r\n            >删除</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <edit-data ref=\"editData\" />\r\n  </div>\r\n</template>\r\n<script>\r\nimport editData from \"./components/editData.vue\";\r\nimport { paylist } from \"@/request/api\";\r\nexport default {\r\n  components: {\r\n    editData,\r\n  },\r\n  data() {\r\n    return {\r\n      userList: [],\r\n      num: \"\",\r\n      need_num: \"\",\r\n      id: \"\",\r\n      coupon_id: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    this.getlist();\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    lishi() {\r\n      this.$router.push({\r\n        path: \"/viphis\",\r\n      });\r\n    },\r\n    add() {\r\n      this.$refs.editData.show();\r\n    },\r\n    getlist() {\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n        type: 1,\r\n      };\r\n      paylist(params).then((res) => {\r\n        console.log(res);\r\n      });\r\n    },\r\n    open(row) {\r\n      console.log(row.id);\r\n      this.id = row.id;\r\n      this.$confirm(\"此操作将永久删除该, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.shanchu();\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    shanchu() {\r\n      var formdata = new FormData();\r\n\r\n      formdata.append(\"id\", this.id);\r\n\r\n      axios\r\n        .post(\"http://y1.wjw.cool//api/member/delAct\", formdata)\r\n        .then((res) => {\r\n          if (res.status == 200) {\r\n            this.$message.success(\"删除成功！\");\r\n            setTimeout(() => {\r\n              window.location.reload();\r\n            }, 2000);\r\n          }\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style  scoped>\r\n.xun {\r\n  font-size: 14px;\r\n  color: rgb(151, 151, 151);\r\n  padding-bottom: 5px;\r\n  padding-top: 10px;\r\n  margin-left: 200px;\r\n}\r\n.tab {\r\n  margin-left: 50px;\r\n}\r\n.bt {\r\n  padding: 10px 30px;\r\n  margin-top: 100px;\r\n  margin-left: 600px;\r\n}\r\n\r\n.xunre {\r\n  margin: 50px 400px;\r\n}\r\n.tijiao {\r\n  margin-top: 110px;\r\n}\r\n</style>\r\n"]}]}