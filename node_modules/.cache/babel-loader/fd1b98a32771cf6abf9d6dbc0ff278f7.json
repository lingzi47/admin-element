AA,OAAA,CADA;AAEA,QAAA,aAAA,EAAA,KAAA,aAFA;AAGA,QAAA,aAAA,EAAA,KAAA,aAHA;AAIA,QAAA,aAAA,EAAA,KAAA,aAJA;AAKA,QAAA,OAAA,EAAA,KAAA,OALA;AAMA,QAAA,MAAA,EAAA,KAAA,MANA;AAOA,QAAA,GAAA,EAAA,KAAA,GAPA;AAQA,QAAA,YAAA,EAAA,KAAA;AARA,OAAA;AAUA,MAAA,QAAA,CAAA,MAAA,EAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,UAAA,aAAA,GAAA,EAAA;AACA,SARA,MAQA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,UAAA,aAAA,GAAA,EAAA;AACA;AACA,OAnBA;AAoBA;AAnFA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"editData\">\r\n    <el-dialog\r\n      title=\"信息修改\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"800px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form label-width=\"auto\" :rules=\"rules\">\r\n        <el-row>\r\n          <el-col :span=\"12\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"封面图\" prop=\"goods_img\">\r\n                <el-upload\r\n                  class=\"avatar-uploader\"\r\n                  action=\"https://y4.wjw.cool/command/ossUpload?filename=file\"\r\n                  :show-file-list=\"false\"\r\n                  :on-success=\"handleAvatarSuccess\"\r\n                  :before-upload=\"beforeAvatarUpload\"\r\n                >\r\n                  <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" /><i\r\n                    v-else\r\n                    class=\"el-icon-plus avatar-uploader-icon\"\r\n                  ></i>\r\n                </el-upload>\r\n              </el-form-item></div\r\n          ></el-col>\r\n          <el-col :span=\"10\" class=\"userif\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <el-form-item label=\"用户id\">\r\n                <el-input v-model=\"userid\" disabled></el-input\r\n              ></el-form-item></div\r\n          ></el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"12\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"原用户身份\" prop=\"member\">\r\n                <el-select\r\n                  style=\"width: 180px\"\r\n                  v-model=\"member\"\r\n                  clearable\r\n                  placeholder=\"请选择用户身份\"\r\n                >\r\n                  <el-option label=\"普通用户\" :value=\"0\"></el-option>\r\n                  <el-option label=\"MT黑卡\" :value=\"1\"></el-option>\r\n                  <el-option label=\"C级推广员\" :value=\"2\"></el-option>\r\n                  <el-option label=\"B级推广员\" :value=\"3\"></el-option>\r\n                  <el-option label=\"A级推广员\" :value=\"4\"></el-option>\r\n                </el-select>\r\n              </el-form-item></div\r\n          ></el-col>\r\n          <el-col :span=\"12\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <el-form-item label=\"用户上级\">\r\n                <el-input v-model=\"pid\"></el-input\r\n              ></el-form-item></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"24\"\r\n            ><div class=\"grid-content bg-purple-dark\">\r\n              <el-form-item label=\"会员到期时间\" v-if=\"member == 1\">\r\n                <el-date-picker\r\n                  v-model=\"overdue_time\"\r\n                  type=\"date\"\r\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                  placeholder=\"选择日期\"\r\n                >\r\n                </el-date-picker>\r\n              </el-form-item></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"8\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"原拥有pk值\"> {{ pk_value }}</el-form-item>\r\n            </div></el-col\r\n          >\r\n          <el-col :span=\"8\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <el-form-item label=\"pk值增加或减少（+/-）\">\r\n                <el-input\r\n                  v-model=\"pk_value_save\"\r\n                  placeholder=\"例：+100\"\r\n                ></el-input>\r\n              </el-form-item></div\r\n          ></el-col>\r\n          <el-col :span=\"10\" class=\"xian\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"备注\">\r\n                <el-input v-model=\"pk_text\" placeholder=\"请输入\"></el-input>\r\n              </el-form-item></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"8\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"原拥有钻石\"> {{ diamonds }}</el-form-item>\r\n            </div></el-col\r\n          >\r\n          <el-col :span=\"8\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <el-form-item label=\"钻石增加或减少（+/-）\">\r\n                <el-input\r\n                  v-model=\"diamonds_save\"\r\n                  placeholder=\"例：+100\"\r\n                ></el-input>\r\n              </el-form-item></div\r\n          ></el-col>\r\n          <el-col :span=\"10\" class=\"xian\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <el-form-item label=\"备注\">\r\n                <el-input\r\n                  v-model=\"diamonds_text\"\r\n                  placeholder=\"请输入\"\r\n                ></el-input>\r\n              </el-form-item></div\r\n          ></el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { edituser, appuserShow } from \"@/request/api\";\r\nexport default {\r\n  name: \"AddDialog\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      userid: \"\",\r\n      imageUrl: \"\",\r\n      pk_text: \"\",\r\n      diamonds_text: \"\",\r\n      token: \"\",\r\n      rolesList: [], //角色列表\r\n      pk_value: \"\",\r\n      pk_value_save: \"\",\r\n      diamonds: \"\",\r\n      overdue_time: \"\",\r\n      diamonds_save: \"\",\r\n      member: \"\",\r\n      dialogVisible: false,\r\n      pid: \"\",\r\n      rules: {\r\n        member: [\r\n          {\r\n            required: true,\r\n            message: \"请选择用户身份\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  created: function () {\r\n    //console.log(this.type);\r\n  },\r\n  mounted: function () {},\r\n  methods: {\r\n    //获取修改的信息\r\n    beforeAvatarUpload(file) {\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传图片大小不能超过 2MB!\");\r\n      }\r\n      return isLt2M;\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n      //console.log(file);\r\n      //console.log(res);\r\n      let imgurl = res.data;\r\n      this.imageUrl = imgurl;\r\n    },\r\n    show(row) {\r\n      //console.log(row);\r\n      this.dialogVisible = true;\r\n      //console.log(row.id);\r\n      let id = row.id;\r\n      console.log(row);\r\n      this.imageUrl = row.head_img;\r\n      this.userid = row.id;\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n      };\r\n      appuserShow(params, id).then((res) => {\r\n        //console.log(res.data.data);\r\n        this.pk_value = res.data.data.pk_value;\r\n        this.diamonds = res.data.data.diamonds;\r\n        this.member = res.data.data.member;\r\n        this.id = res.data.data.id;\r\n        this.overdue_time = res.data.data.overdue_time;\r\n        this.pid = res.data.data.pid;\r\n      });\r\n    },\r\n    close() {\r\n      this.dialogVisible = false;\r\n    },\r\n    submitForm() {\r\n      //console.log(id);\r\n      if (this.member == 1) {\r\n        if (this.overdue_time == \"\") {\r\n          this.$message.error(\"请选择会员到期时间\");\r\n        } else {\r\n          this.admin();\r\n        }\r\n      } else {\r\n        this.admin();\r\n      }\r\n    },\r\n    admin() {\r\n      let id = this.id;\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n        diamonds_save: this.diamonds_save,\r\n        diamonds_text: this.diamonds_text,\r\n        pk_value_save: this.pk_value_save,\r\n        pk_text: this.pk_text,\r\n        member: this.member,\r\n        pid: this.pid,\r\n        overdue_time: this.overdue_time,\r\n      };\r\n      edituser(params, id).then((res) => {\r\n        if (res.data.code == 200) {\r\n          this.$message.success(\"编辑成功\");\r\n          this.$parent.getUserList();\r\n          this.dialogVisible = false;\r\n          this.pk_value_save = \"\";\r\n          this.diamonds_save = \"\";\r\n          this.pk_text = \"\";\r\n          diamonds_text = \"\";\r\n        } else {\r\n          this.$message(res.data.msg);\r\n          this.$parent.getUserList();\r\n          this.dialogVisible = false;\r\n          this.pk_value_save = \"\";\r\n          this.diamonds_save = \"\";\r\n          this.overdue_time = \"\";\r\n          this.pk_text = \"\";\r\n          diamonds_text = \"\";\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n.xian {\r\n  margin-left: -75px !important;\r\n}\r\n.userif {\r\n  margin-top: 100px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/user/appuserset/userlist/components"}]}