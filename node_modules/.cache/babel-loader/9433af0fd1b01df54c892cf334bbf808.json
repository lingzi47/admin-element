A,QAAA,GAAA,GAAA;AACA,KALA;AAMA,IAAA,KANA,mBAMA;AAAA;;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,aAAA,GADA,CACA;;AACA,OAFA;AAGA,KAXA;AAYA,IAAA,mBAZA,+BAYA,GAZA,EAYA,IAZA,EAYA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,UAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,WAAA,QAAA,GAAA,MAAA;AACA,WAAA,QAAA,CAAA,GAAA,GAAA,MAAA;AACA,KAlBA;AAmBA,IAAA,kBAnBA,8BAmBA,IAnBA,EAmBA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,CAAA;;AAEA,UAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA;;AACA,aAAA,MAAA;AACA,KA1BA;AA4BA,IAAA,UA5BA,wBA4BA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA;AAAA,2EAAA,iBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,SAAA,GAAA,CAAA;AACA,yBAAA,MAAA,CAAA,QAAA,CAAA,aAAA,CAAA;AACA,yBAAA,MAAA,CAAA,QAAA,CAAA,aAAA,CAAA;AACA,yBAAA,MAAA,CAAA,QAAA,CAAA,aAAA,CAAA;;AAJA,uBAKA,KALA;AAAA;AAAA;AAAA;;AAAA,wBAMA,MAAA,CAAA,IAAA,IAAA,CANA;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAOA,WAAA,CAAA,MAAA,CAAA,QAAA,CAPA;;AAAA;AAOA,kBAAA,GAPA;AAQA,sBAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KACA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AATA;AAAA;;AAAA;AAAA,wBAUA,MAAA,CAAA,IAAA,IAAA,CAVA;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAWA,YAAA,CAAA,MAAA,CAAA,QAAA,CAXA;;AAAA;AAWA,kBAAA,IAXA;AAYA,sBAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AAZA;AAAA;;AAAA;AAcA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,IAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,yBAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AAfA;AAAA,yBAgBA,cAAA,CAAA,MAAA,CAAA,QAAA,CAhBA;;AAAA;AAgBA,kBAAA,KAhBA;AAiBA,sBAAA,KAAA,CAAA,MAAA,IAAA,GAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,KAAA,CAAA,OAAA;;AAjBA;AAmBA,kBAAA,MAAA,CAAA,OAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,KAAA;;AApBA;AAAA;;AAAA;AAAA,mDAsBA,KAtBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAyBA;AAtDA;AAlBA,CAAA","sourcesContent":["<template>\n  <div class=\"roleAddOrEditDialog\">\n    <el-dialog\n      :title=\"type == 1 ? '新增' : '编辑'\"\n      :visible.sync=\"dialogVisible\"\n      width=\"400px\"\n      :close-on-click-modal=\"false\"\n      @close=\"close\"\n    >\n      <el-form\n        :model=\"ruleForm\"\n        ref=\"ruleForm\"\n        :rules=\"rules\"\n        label-width=\"auto\"\n      >\n        <el-input type=\"hidden\" v-model=\"ruleForm.type_id\"></el-input>\n        <el-row :gutter=\"20\">\n          <el-col :span=\"24\">\n            <el-form-item label=\"分类名称\" prop=\"name\">\n              <el-input v-model=\"ruleForm.type_name\" placeholder=\"请输入分类\">\n              </el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"24\">\n            <el-form-item label=\"排序(大->小)\" prop=\"title\">\n              <el-input\n                v-model=\"ruleForm.weigh\"\n                placeholder=\"排序(大->小)\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"24\">\n            <el-form-item label=\"分类简介\" prop=\"description\">\n              <el-input\n                v-model=\"ruleForm.brief\"\n                placeholder=\"请输入简介\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n\n          <el-col :span=\"24\">\n            <el-form-item label=\"图标\" prop=\"description\">\n              <el-upload\n                class=\"avatar-uploader\"\n                action=\"http://y4.wjw.cool/command/ossUpload?filename=file\"\n                :show-file-list=\"false\"\n                :on-success=\"handleAvatarSuccess\"\n                :before-upload=\"beforeAvatarUpload\"\n              >\n                <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\n                <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n              </el-upload>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { shopTypeadd, shopTypeedit, shopTyptreeadd } from \"@/api/shop\";\nexport default {\n  name: \"roleAddOrEditDialog\",\n  components: {},\n  data() {\n    return {\n      imageUrl: \"\",\n      type: 1, //1新增，2编辑，3分类新增\n      dialogVisible: false,\n      ruleForm: {},\n      rules: {\n        type_name: [\n          { required: true, message: \"分类名称不能为空\", trigger: \"blur\" },\n        ],\n      },\n    };\n  },\n  created: function () {},\n  mounted: function () {},\n  methods: {\n    show(type, row) {\n      this.type = type;\n      this.dialogVisible = true;\n      this.ruleForm = row;\n    },\n    close() {\n      this.dialogVisible = false;\n      this.$nextTick(() => {\n        this.$refs.ruleForm.clearValidate(); //关闭清空校验规则\n      });\n    },\n    handleAvatarSuccess(res, file) {\n      console.log(\"file\", file);\n      console.log(\"res\", res);\n      let imgurl = res.data;\n      this.imageUrl = imgurl;\n      this.ruleForm.img = imgurl;\n    },\n    beforeAvatarUpload(file) {\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isLt2M) {\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\n      }\n      return isLt2M;\n    },\n\n    submitForm() {\n      this.$refs.ruleForm.validate(async (valid) => {\n        this.ruleForm.shop_type = 4;\n        delete this.ruleForm[\"create_time\"];\n        delete this.ruleForm[\"update_time\"];\n        delete this.ruleForm[\"delete_time\"];\n        if (valid) {\n          if (this.type == 1) {\n            let res = await shopTypeadd(this.ruleForm);\n            if (res.status == 200) this.$message.success(res.message);\n            else this.$message.warning(res.message);\n          } else if (this.type == 2) {\n            let res = await shopTypeedit(this.ruleForm);\n            if (res.status == 200) this.$message.success(res.message);\n          } else {\n            this.ruleForm[\"pid\"] = this.ruleForm[\"id\"];\n            delete this.ruleForm[\"id\"];\n            let res = await shopTyptreeadd(this.ruleForm);\n            if (res.status == 200) this.$message.success(res.message);\n          }\n          this.$parent.getlist();\n          this.close();\n        } else {\n          return false;\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style>\n.avatar-uploader .el-upload {\n  border: 1px dashed #d9d9d9;\n  border-radius: 6px;\n  cursor: pointer;\n  position: relative;\n  overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409eff;\n}\n.avatar-uploader-icon {\n  font-size: 28px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 178px;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n}\n</style>\n"],"sourceRoot":"src/views/shop/extend/type"}]}