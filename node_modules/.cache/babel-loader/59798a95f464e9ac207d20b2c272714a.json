AA,KAFA,GAEA,cAAA,CAAA,OAAA,CAAA,OAAA,CAFA;AAGA,oBAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,oBAAA,MAJA,GAIA;AACA,sBAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,IADA;AAEA,sBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAFA;AAGA,sBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CAHA;AAIA,sBAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,IAJA;AAKA,sBAAA,EAAA,EAAA,MAAA,CAAA,EALA;AAMA,sBAAA,MAAA,EAAA,MAAA,CAAA,QAAA,CAAA,MANA;AAOA,sBAAA,OAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAPA;AAQA,sBAAA,QAAA,EAAA,MAAA,CAAA,QARA;AASA,sBAAA,IAAA,EAAA,MAAA,CAAA,IATA;AAUA,sBAAA,IAAA,EAAA,MAAA,CAAA;AAVA,qBAJA;AAgBA,oBAAA,YAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,0BAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,wBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,wBAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,wBAAA,MAAA,CAAA,KAAA;;AACA,wBAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,uBALA,MAKA;AACA,wBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;;AACA,wBAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,wBAAA,MAAA,CAAA,KAAA;;AACA,wBAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA;AACA,qBAZA;AAhBA;AAAA;;AAAA;AAAA,sDA8BA,KA9BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,WAAA;;AAAA;AAAA;AAAA;AAAA;AAiCA;AACA;AA3HA;AAxDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-dialog\r\n      class=\"AddDialog\"\r\n      :title=\"tittle\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"800px\"\r\n      hegiht=\"1000px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form\r\n        :model=\"ruleForm\"\r\n        ref=\"ruleForm\"\r\n        :rules=\"rules\"\r\n        label-width=\"auto\"\r\n      >\r\n        <el-row :gutter=\"15\">\r\n          <el-col :span=\"15\">\r\n            <el-form-item label=\"药房名称\" prop=\"name\">\r\n              <el-input\r\n                v-model=\"ruleForm.name\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入药房名称\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"20\">\r\n            <el-form-item label=\"药房位置\" prop=\"value1\">\r\n              <el-cascader\r\n                style=\"width: 250px\"\r\n                v-model=\"ruleForm.value1\"\r\n                :options=\"areaArr\"\r\n                :props=\"{ value: 'name', label: 'name' }\"\r\n                placeholder=\"请选择省市区\"\r\n                filterable\r\n                @change=\"change\"\r\n              ></el-cascader>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"药房详细位置\" prop=\"details\">\r\n              <el-input\r\n                v-model=\"ruleForm.details\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入药房详细位置\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"药房负责人\" prop=\"person\">\r\n              <el-input\r\n                v-model=\"ruleForm.person\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入药房负责人\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"药房联系方式\" prop=\"phone\">\r\n              <el-input\r\n                v-model=\"ruleForm.phone\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入药房联系方式\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"拨款银行账户\" prop=\"bank\">\r\n              <el-input\r\n                v-model=\"ruleForm.bank\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入拨款银行账户\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { pharmacyadd, pharmacyedit, goodsedit } from \"@/request/api\";\r\nimport { areaListData } from \"@/utils/area\";\r\nexport default {\r\n  name: \"AddDialog\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      check: false,\r\n      token: \"\",\r\n      isDisable: false,\r\n      tittle: \"\",\r\n      form: {\r\n        value1: \"\",\r\n        value2: \"\",\r\n        value3: \"\",\r\n      },\r\n      province: \"\",\r\n      type: \"\",\r\n      city: \"\",\r\n      id: \"\",\r\n      area: \"\",\r\n      type: \"\", //1新增，2编辑\r\n      dialogVisible: false,\r\n      ruleForm: {\r\n        value1: \"\",\r\n        phone: \"\",\r\n        name: \"\",\r\n        bank: \"\",\r\n        person: \"\",\r\n        details: \"\",\r\n      },\r\n      rules: {\r\n        person: [\r\n          { required: true, message: \"请输入负责人姓名\", trigger: \"blur\" },\r\n        ],\r\n        value1: [{ required: true, message: \"请选择省市区\", trigger: \"blur\" }],\r\n        phone: [\r\n          { required: true, message: \"请输入手机号\", trigger: \"blur\" },\r\n          {\r\n            pattern: /^1[3456789]\\d{9}$/,\r\n            message: \"手机号格式不正确\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        details: [\r\n          { required: true, message: \"请输入详情信息\", trigger: \"blur\" },\r\n        ],\r\n        name: [{ required: true, message: \"请输入药房名称\", trigger: \"blur\" }],\r\n        bank: [{ required: true, message: \"请输入银行卡号\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.setData(areaListData());\r\n    this.areaArr = areaListData();\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    setData(data) {\r\n      data.map((item) => {\r\n        item[\"value\"] = item.code;\r\n        item[\"label\"] = item.name;\r\n        if (item.children) {\r\n          this.setData(item.children);\r\n        }\r\n      });\r\n    },\r\n    change(data) {\r\n      console.log(data);\r\n      this.province = data[0];\r\n      this.city = data[1];\r\n      this.area = data[2];\r\n    },\r\n    show(type, row) {\r\n      this.dialogVisible = true;\r\n      console.log(type);\r\n      this.type = type;\r\n      if (this.type == 2) {\r\n        this.tittle = \"编辑\";\r\n        console.log(row);\r\n        this.id = row.id;\r\n        this.ruleForm.phone = row.phone;\r\n        this.ruleForm.bank = row.bank;\r\n        this.ruleForm.person = row.person;\r\n        this.ruleForm.name = row.name;\r\n        this.ruleForm.details = row.details;\r\n        // this.ruleForm.value1 = \"\";\r\n        let arr1 = [];\r\n        arr1.push(row.province);\r\n        arr1.push(row.city);\r\n        arr1.push(row.area);\r\n\r\n        console.log(arr1);\r\n        this.ruleForm.value1 = arr1;\r\n      } else {\r\n        this.tittle = \"添加\";\r\n      }\r\n    },\r\n\r\n    close() {\r\n      this.dialogVisible = false;\r\n      this.ruleForm.phone = \"\";\r\n      this.ruleForm.bank = \"\";\r\n      this.ruleForm.person = \"\";\r\n      this.ruleForm.name = \"\";\r\n      this.ruleForm.details = \"\";\r\n      this.isDisable = false;\r\n      this.check = false;\r\n      this.ruleForm.value1 = \"\";\r\n    },\r\n\r\n    submitForm() {\r\n      if (this.type == 1) {\r\n        this.$refs.ruleForm.validate(async (valid) => {\r\n          if (valid) {\r\n            let token = sessionStorage.getItem(\"token\");\r\n            this.token = token;\r\n            let params = {\r\n              bank: this.ruleForm.bank,\r\n              phone: this.ruleForm.phone,\r\n              token: sessionStorage.getItem(\"token\"),\r\n              name: this.ruleForm.name,\r\n              person: this.ruleForm.person,\r\n              details: this.ruleForm.details,\r\n              province: this.province,\r\n              city: this.city,\r\n              area: this.area,\r\n            };\r\n            pharmacyadd(params).then((res) => {\r\n              if (res.data.code == 200) {\r\n                this.$message.success(\"新增成功\");\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              }\r\n            });\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      } else {\r\n        this.$refs.ruleForm.validate(async (valid) => {\r\n          if (valid) {\r\n            let token = sessionStorage.getItem(\"token\");\r\n            this.token = token;\r\n            let params = {\r\n              bank: this.ruleForm.bank,\r\n              phone: this.ruleForm.phone,\r\n              token: sessionStorage.getItem(\"token\"),\r\n              name: this.ruleForm.name,\r\n              id: this.id,\r\n              person: this.ruleForm.person,\r\n              details: this.ruleForm.details,\r\n              province: this.province,\r\n              city: this.city,\r\n              area: this.area,\r\n            };\r\n            pharmacyedit(params).then((res) => {\r\n              if (res.data.code == 200) {\r\n                this.$message.success(\"新增成功\");\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              }\r\n            });\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.xian {\r\n  margin-left: -75px !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/inven/invenlist/components"}]}