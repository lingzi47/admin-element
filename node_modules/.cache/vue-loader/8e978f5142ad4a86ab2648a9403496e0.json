en\"),\r\n        };\r\n        placeedit(params).then((res) => {\r\n          if (res.data.code == 200) {\r\n            this.$message.success(\"修改成功\");\r\n            this.close();\r\n            this.$parent.getUserList();\r\n          } else {\r\n            this.$message(res.data.msg);\r\n            this.close();\r\n            this.$parent.getUserList();\r\n          }\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}