"false\">\n            <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取文件</el-button>\n            <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\n          </el-upload>\n        </el-form-item>\n        <el-form-item label=\"上传图片：\">\n          <el-upload ref=\"upload\" action=\"#\" list-type=\"picture-card\" :auto-upload=\"false\" class=\"upload\">\n            <i class=\"el-icon-plus\"></i>\n          </el-upload>\n          <el-button style=\"margin-left: 10px;\" size=\"small\" type=\"success\" @click=\"submitUpload\">上传到服务器</el-button>\n        </el-form-item>\n        <el-form-item label=\"文本域：\" prop=\"textarea\">\n          <el-input type=\"textarea\" v-model=\"form.textarea\" rows=\"3\" placeholder=\"请输入\"></el-input>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"onSubmit\">确定</el-button>\n          <el-button @click=\"resetForm\">重置</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { areaListData } from \"@/utils/area\";\nexport default {\n  data() {\n    return {\n      form: {\n        text: \"\",\n        radio: \"1\",\n        checkbox: [false, false, false],\n        select: \"\",\n        filterableSelect: \"\",\n        multipleSelect: \"\",\n        cascader: \"\",\n        date: \"\",\n        dateTime: \"\",\n        dateRange: \"\",\n        inputNumber: 0,\n        switch: false,\n        slider: 0,\n        rate: null,\n        color: \"\",\n        textarea: \"\"\n      },\n      roleList: [], //所有省市区\n    }\n  },\n  created() {\n    this.roleList = areaListData();\n  },\n  mounted() { },\n  methods: {\n    submitUpload() {  // 点击上传文件\n      this.$refs.upload.submit();\n    },\n    onSubmit() {//点击确定\n      console.log(JSON.parse(JSON.stringify(this.form)));\n    },\n    resetForm() {  //重置表单\n      this.$refs[\"form\"].resetFields();\n    }\n  },\n}\n<