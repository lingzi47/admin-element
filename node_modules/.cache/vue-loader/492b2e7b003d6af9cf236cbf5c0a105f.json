l-descriptions-item>\r\n      </el-descriptions>\r\n    </div>\r\n    <hr />\r\n    <!-- 列表 -->\r\n    <div class=\"user\">\r\n      <div class=\"block-quote\">\r\n        <el-form :inline=\"true\" ref=\"form\">\r\n          <el-form-item label=\"用户id\" prop=\"id\">\r\n            <el-input\r\n              style=\"width: 180px\"\r\n              v-model=\"form.id\"\r\n              clearable\r\n              placeholder=\"请输入用户id\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"用户名称\" prop=\"username\">\r\n            <el-input\r\n              style=\"width: 180px\"\r\n              v-model=\"form.username\"\r\n              clearable\r\n              placeholder=\"请输入用户名称\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"用户身份\" prop=\"member\">\r\n            <el-select\r\n              style=\"width: 180px\"\r\n              v-model=\"form.member\"\r\n              clearable\r\n              placeholder=\"请选择用户身份\"\r\n            >\r\n              <el-option label=\"普通用户\" :value=\"0\"></el-option>\r\n              <el-option label=\"MT黑卡\" :value=\"1\"></el-option>\r\n              <el-option label=\"C级推广员\" :value=\"2\"></el-option>\r\n              <el-option label=\"B级推广员\" :value=\"3\"></el-option>\r\n              <el-option label=\"A级推广员\" :value=\"4\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"注册时间\" prop=\"time\">\r\n            <el-date-picker\r\n              v-model=\"form.time\"\r\n              type=\"daterange\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始日期\"\r\n              end-placeholder=\"结束日期\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item style=\"float: right\">\r\n            <el-button\r\n              v-if=\"checkPermission('operationsearch')\"\r\n              type=\"primary\"\r\n              icon=\"el-icon-search\"\r\n              @click=\"shoporder\"\r\n              >查询</el-button\r\n            >\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <page-table\r\n        ref=\"dataTable\"\r\n        :data=\"shoporder\"\r\n        @changeCurrentPage=\"changeCurrent\"\r\n      >\r\n        <el-table-column\r\n          type=\"index\"\r\n          fixed\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"id\"\r\n          fixed\r\n          label=\"ID\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"order_no\"\r\n          fixed\r\n          label=\"订单号\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"goods_id\"\r\n          fixed\r\n          label=\"商品名称\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"sku_id\"\r\n          fixed\r\n          align=\"center\"\r\n          label=\"商品规格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"order_price\"\r\n          fixed\r\n          label=\"订单金额\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"postage_price\"\r\n          fixed\r\n          label=\"邮费\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"coupon_price\"\r\n          fixed\r\n          label=\"代金券\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"pay_price\"\r\n          fixed\r\n          label=\"支付金额\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"pay_status\"\r\n          fixed\r\n          label=\"支付状态\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"deliver_status\"\r\n          fixed\r\n          label=\"发货状态\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"pay_time\"\r\n          fixed\r\n          align=\"center\"\r\n          label=\"支付时间\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"delivery\"\r\n          fixed\r\n          align=\"center\"\r\n          label=\"快递\"\r\n          :resizable=\"false\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column fixed label=\"快递单号\" :resizable=\"false\">\r\n          <template slot-scope=\"scope\">\r\n            <el-input\r\n              v-model=\"scope.row.express\"\r\n              @change=\"onInputChange(scope.row)\"\r\n            >\r\n            </el-input>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n          v-if=\"checkPermission(['useredit', 'userdelete'])\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-link\r\n              v-if=\"checkPermission('useredit')\"\r\n              type=\"primary\"\r\n              @click=\"open(scope.row)\"\r\n              >发货</el-link\r\n            >\r\n            <el-link\r\n              v-if=\"checkPermission('userdelete')\"\r\n              type=\"danger\"\r\n              style=\"margin-left: 10px\"\r\n              >退款</el-link\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </page-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { checkPermission } from \"@/utils/permissions\";\r\nimport { shopordercensus, shoporderindex } from \"@/api/shop\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\nexport default {\r\n  name: \"specialorder\",\r\n  components: {\r\n    pageTable,\r\n  },\r\n  data() {\r\n    return {\r\n      census: [],\r\n      shoporder: [],\r\n      tableSelectList: [],\r\n\r\n      form: {\r\n        username: \"\",\r\n        member: \"\",\r\n        time: \"\",\r\n        id: \"\",\r\n      },\r\n      page: {\r\n        //分页信息\r\n        page: 1, //当前页\r\n        limit: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n      pickerOptions: {\r\n        shortcuts: [\r\n          {\r\n            text: \"最近一周\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            },\r\n          },\r\n          {\r\n            text: \"最近一个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            },\r\n          },\r\n          {\r\n            text: \"最近三个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      value1: [new Date(2000, 10, 10, 10, 10), new Date(2000, 10, 11, 10, 10)],\r\n      value2: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    this.shoporderlist();\r\n    this.ordercensus();\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    onInputChange(row) {\r\n      console.log(row.express);\r\n    },\r\n    checkPermission,\r\n    async ordercensus() {\r\n      let res = await shopordercensus();\r\n      if (res.status == 200) {\r\n        this.census = res.data.data;\r\n        console.log(this.census);\r\n      }\r\n    },\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getUserList();\r\n    },\r\n\r\n    async shoporderlist() {\r\n      let res = await shoporderindex();\r\n      if (res.status == 200) {\r\n        this.shoporder = res.data.data;\r\n      }\r\n    },\r\n\r\n    open(row) {\r\n      console.log(row.express);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.census {\r\n  padding-left: 80px;\r\n}\r\n</style>\r\n"]}]}