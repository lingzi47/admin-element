;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/broker/brokerlist","sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <div class=\"block-quote\">\r\n      <el-form :inline=\"true\">\r\n        <el-form-item label=\"设备租赁号\" prop=\"name\">\r\n          <el-input\r\n            style=\"width: 180px\"\r\n            v-model=\"name\"\r\n            clearable\r\n            placeholder=\"请输入设备租赁号\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"推荐人id\" prop=\"use_pid\">\r\n          <el-input\r\n            style=\"width: 180px\"\r\n            v-model=\"use_pid\"\r\n            clearable\r\n            placeholder=\"请输入推荐人id\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"推荐人租赁号\" prop=\"pid\">\r\n          <el-input\r\n            style=\"width: 180px\"\r\n            v-model=\"pid\"\r\n            clearable\r\n            placeholder=\"请输入推荐人租赁号\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"购买服务\" prop=\"box_type\">\r\n          <el-select\r\n            v-model=\"box_type\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 150px\"\r\n          >\r\n            <el-option label=\"全部状态\" value=\"\"></el-option>\r\n            <el-option label=\"终身\" value=\"3\"></el-option>\r\n            <el-option label=\"三年\" value=\"1\"></el-option>\r\n            <el-option label=\"五年\" value=\"2\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"租赁号状态\" prop=\"status\">\r\n          <el-select v-model=\"status\" placeholder=\"请选择\" style=\"width: 150px\">\r\n            <el-option label=\"全部状态\" value=\"\"></el-option>\r\n            <el-option label=\"已绑定\" value=\"20\"></el-option>\r\n            <el-option label=\"排队中\" value=\"10\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核状态\" prop=\"status\">\r\n          <el-select\r\n            v-model=\"ex_status\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 150px\"\r\n          >\r\n            <el-option label=\"全部状态\" value=\"\"></el-option>\r\n            <el-option label=\"通过\" value=\"2\"></el-option>\r\n            <el-option label=\"待审核\" value=\"1\"></el-option>\r\n            <el-option label=\"拒绝\" value=\"3\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"是否真实购买\" prop=\"status\">\r\n          <el-select\r\n            v-model=\"device_type\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 150px\"\r\n          >\r\n            <el-option label=\"是\" value=\"10\"></el-option>\r\n            <el-option label=\"否\" value=\"20\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item style=\"float: right\">\r\n          <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"searchinfo\"\r\n            >搜索</el-button\r\n          >\r\n          <el-button type=\"primary\" @click=\"add\">手动添加租赁人</el-button>\r\n          <el-button @click=\"dao\">导出</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <page-table\r\n      ref=\"dataTable\"\r\n      :data=\"userList\"\r\n      @changeCurrentPage=\"changeCurrent\"\r\n    >\r\n      <el-table-column label=\"序号\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{\r\n            (page.currentPage - 1) * page.pageSize + scope.$index + 1\r\n          }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"name\" label=\"设备租赁号\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"box_number\" label=\"设备编号\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column label=\"是否真实购买\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-if=\"scope.row.device_type == 10\">是</span>\r\n          <span v-if=\"scope.row.device_type == 20\">否</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"租赁人数\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link @click=\"handleClick(scope.row)\">{{\r\n            scope.row.count\r\n          }}</el-link>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"推荐人租赁号\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-if=\"scope.row.pid == null\">无</span>\r\n          <span v-else>{{ scope.row.pid }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"use_pid\" label=\"推荐人\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column label=\"购买药柜租赁服务\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-if=\"scope.row.box_type == 1\">三年</span>\r\n          <span v-if=\"scope.row.box_type == 2\">五年</span>\r\n          <span v-if=\"scope.row.box_type == 3\">终身</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"购买区域\" align=\"ce nter\" :resizable=\"false\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link v-if=\"scope.row.box_team == 1\">全国</el-link>\r\n          <el-link v-if=\"scope.row.box_team == 2\">大连</el-link>\r\n          <el-link v-if=\"scope.row.box_team == 3\">大庆</el-link>\r\n          <el-link v-if=\"scope.row.box_team == 4\">北京</el-link>\r\n          <el-link type=\"danger\" v-if=\"scope.row.box_team == null\"\r\n            >暂无</el-link\r\n          >\r\n        </template></el-table-column\r\n      >\r\n      <el-table-column prop=\"queue\" label=\"队列编号\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"审核状态\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link type=\"success\" v-if=\"scope.row.ex_status == 2\"\r\n            >已通过</el-link\r\n          >\r\n          <el-link type=\"danger\" v-if=\"scope.row.ex_status == 3\"\r\n            >未通过</el-link\r\n          >\r\n          <el-link type=\"primary\" v-if=\"scope.row.ex_status == 1\"\r\n            >待审核</el-link\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"状态\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link type=\"success\" v-if=\"scope.row.status == 20\">已绑定</el-link>\r\n          <el-link type=\"danger\" v-if=\"scope.row.status == 10\">排队中</el-link>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"350\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link\r\n            type=\"primary\"\r\n            @click=\"showtable(scope.row)\"\r\n            style=\"margin-left: 10px\"\r\n            >查看应急箱</el-link\r\n          >\r\n\r\n          <el-link\r\n            type=\"success\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"fenrun(scope.row)\"\r\n            >商务团队信息</el-link\r\n          >\r\n\r\n          <el-link\r\n            type=\"warning\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"addup(1, scope.row)\"\r\n            v-if=\"scope.row.pid == 0 && scope.row.ex_status == 2\"\r\n            >绑定上级</el-link\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </page-table>\r\n    <el-dialog\r\n      title=\"账户信息\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"600px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-table ref=\"dataTable\" :data=\"List\" border>\r\n        <el-table-column label=\"序号\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{\r\n              (page.currentPage - 1) * page.pageSize + scope.$index + 1\r\n            }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"uid\" label=\"用户id\" align=\"center\">\r\n        </el-table-column>\r\n\r\n        <el-table-column prop=\"phone\" label=\"联系方式\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"share\" label=\"分润占比\" align=\"center\">\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" size=\"small\" @click=\"edit(scope.row)\"\r\n              >编辑</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确认</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <el-dialog\r\n      title=\"信息修改\"\r\n      :visible.sync=\"Visible\"\r\n      width=\"600px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"closee\"\r\n    >\r\n      <el-form :rules=\"rules\" label-width=\"auto\" :model=\"Form\" ref=\"Form\">\r\n        <el-row :gutter=\"24\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"用户id:\" prop=\"uid\">\r\n              <el-input\r\n                disabled\r\n                v-model=\"Form.uid\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"联系方式:\" prop=\"phone\">\r\n              <el-input\r\n                v-model=\"Form.phone\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"分润占比:\" prop=\"share\">\r\n              <el-select\r\n                disabled\r\n                v-model=\"Form.share\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 100px\"\r\n              >\r\n                <el-option label=\"12.5%\" value=\"12.50\"></el-option>\r\n                <el-option label=\"25%\" value=\"25.00\"></el-option>\r\n                <el-option label=\"50%\" value=\"50.00\"></el-option>\r\n                <el-option label=\"100%\" value=\"100.00\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"closee\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submit\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 新增编辑弹窗 -->\r\n    <edit-data ref=\"editData\" />\r\n    <fen-run ref=\"fenRun\" />\r\n    <up-set ref=\"upSet\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { doctorlist, upPid, shareInfo, shareedit } from \"@/request/api\";\r\nimport { checkPermission } from \"@/utils/permissions\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\nimport editData from \"./components/editData.vue\";\r\nimport fenRun from \"./components/fenRun.vue\";\r\nimport upSet from \"./components/upSet.vue\";\r\nexport default {\r\n  name: \"user\",\r\n  components: {\r\n    pageTable,\r\n    editData,\r\n    fenRun,\r\n    upSet,\r\n  },\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      Visible: false,\r\n      name: \"\",\r\n      uid: \"\",\r\n      pid: \"\",\r\n      use_pid: \"\",\r\n      box_type: \"\",\r\n      box_name: \"\",\r\n      status: \"\",\r\n      ex_status: \"\",\r\n\r\n      device_type: \"\",\r\n      buy_time: \"\",\r\n      userList: [], // 列表\r\n      List: [], // 列表\r\n      Form: {\r\n        id: \"\",\r\n        uid: \"\",\r\n        phone: \"\",\r\n        share: \"\",\r\n      },\r\n      page: {\r\n        //分页信息\r\n        currentPage: 1, //当前页\r\n        pageSize: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n      rules: {\r\n        phone: [\r\n          { required: true, message: \"请输入手机号\", trigger: \"blur\" },\r\n          {\r\n            pattern: /^1[3456789]\\d{9}$/,\r\n            message: \"手机号格式不正确\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        share: [{ required: true, message: \"请选择分润占比\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  watch: {\r\n    buy_time(newVal) {\r\n      if (newVal == null) {\r\n        this.buy_time = [];\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getUserList(); //获取用户列表\r\n  },\r\n  mounted() {},\r\n  computed: {},\r\n  methods: {\r\n    dao() {\r\n      window.location.href =\r\n        \"https://y4.wjw.cool/admin/box/exportList\" +\r\n        \"?token=\" +\r\n        this.token +\r\n        \"&name=\" +\r\n        this.name +\r\n        \"&pid=\" +\r\n        this.pid +\r\n        \"&box_type=\" +\r\n        this.box_type +\r\n        \"&use_pid=\" +\r\n        this.use_pid +\r\n        \"&status=\" +\r\n        this.status +\r\n        \"&device_type=\" +\r\n        this.device_type +\r\n        \"&ex_status=\" +\r\n        this.ex_status;\r\n    },\r\n    submit() {\r\n      let params = {\r\n        id: this.Form.id,\r\n        phone: this.Form.phone,\r\n        share: this.Form.share,\r\n        token: sessionStorage.getItem(\"token\"),\r\n      };\r\n      shareedit(params).then((res) => {\r\n        if (res.data.code == 200) {\r\n          this.$message.success(\"编辑成功\");\r\n          this.closee();\r\n          this.dialogVisible = true;\r\n          this.getList();\r\n        } else {\r\n          this.$message.error(res.data.msg);\r\n          this.closee();\r\n          this.dialogVisible = true;\r\n          this.getList();\r\n        }\r\n      });\r\n    },\r\n    closee() {\r\n      this.Form.id = \"\";\r\n      this.Form.uid = \"\";\r\n      this.Form.phone = \"\";\r\n      this.Form.share = \"\";\r\n      this.Visible = false;\r\n      this.dialogVisible = true;\r\n      // this.getUserList();\r\n    },\r\n    edit(row) {\r\n      console.log(row);\r\n      this.Form.id = row.id;\r\n      this.Form.uid = row.uid;\r\n      this.Form.phone = row.phone;\r\n      this.Form.share = row.share;\r\n      this.Visible = true;\r\n    },\r\n    handleClick(row) {\r\n      console.log(row);\r\n      this.box_name = row.name;\r\n      this.getList();\r\n    },\r\n    getList() {\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n        box_name: this.box_name,\r\n      };\r\n      shareInfo(params).then((res) => {\r\n        console.log(res);\r\n        this.List = res.data.data;\r\n        console.log(this.List);\r\n      });\r\n      this.dialogVisible = true;\r\n    },\r\n    submitForm() {\r\n      this.dialogVisible = false;\r\n    },\r\n\r\n    close() {\r\n      this.dialogVisible = false;\r\n    },\r\n    add() {\r\n      this.$refs.editData.show(1, {});\r\n    },\r\n    addup(type, row) {\r\n      let rowData = row;\r\n      this.$refs.upSet.show(1, JSON.parse(JSON.stringify(rowData)));\r\n    },\r\n    setup(type, row) {\r\n      // console.log(type);\r\n      let rowData = row;\r\n      this.$refs.upSet.show(2, JSON.parse(JSON.stringify(rowData)));\r\n    },\r\n    fenrun(row) {\r\n      let rowData = row;\r\n      this.$refs.fenRun.show(JSON.parse(JSON.stringify(rowData)));\r\n    },\r\n    showtable(row) {\r\n      this.$router.push({\r\n        path: \"/showtable\",\r\n        query: {\r\n          row: row,\r\n        },\r\n      });\r\n    },\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getUserList();\r\n    },\r\n    searchinfo() {\r\n      let token = sessionStorage.getItem(\"token\");\r\n      this.token = token;\r\n      let params = {\r\n        page: 1,\r\n        limit: this.page.pageSize,\r\n        token: sessionStorage.getItem(\"token\"),\r\n        name: this.name,\r\n        pid: this.pid,\r\n        box_type: this.box_type,\r\n\r\n        use_pid: this.use_pid,\r\n        status: this.status,\r\n        ex_status: this.ex_status,\r\n\r\n        device_type: this.device_type,\r\n      };\r\n      doctorlist(params).then((res) => {\r\n        this.page.total = res.data.count;\r\n        this.userList = res.data.data;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: this.page.total,\r\n        });\r\n      });\r\n    },\r\n    getUserList() {\r\n      let token = sessionStorage.getItem(\"token\");\r\n      this.token = token;\r\n      let params = {\r\n        page: this.page.currentPage,\r\n        limit: this.page.pageSize,\r\n        token: sessionStorage.getItem(\"token\"),\r\n        name: this.name,\r\n        pid: this.pid,\r\n        box_type: this.box_type,\r\n\r\n        use_pid: this.use_pid,\r\n        status: this.status,\r\n        ex_status: this.ex_status,\r\n\r\n        device_type: this.device_type,\r\n      };\r\n      doctorlist(params).then((res) => {\r\n        console.log(res.data.data);\r\n        this.page.total = res.data.count;\r\n        this.userList = res.data.data;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: this.page.total,\r\n        });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}