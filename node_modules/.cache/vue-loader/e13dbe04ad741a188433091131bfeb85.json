r\n    },\r\n    submitForm1() {\r\n      this.dialogVisible1 = false;\r\n    },\r\n    quxiao() {\r\n      this.dialogVisible1 = false;\r\n    },\r\n    close1() {\r\n      this.dialogVisible1 = false;\r\n    },\r\n    checked(events) {\r\n      console.log(events);\r\n      this.events = events;\r\n      if (events == 10) {\r\n        this.isDisable = true;\r\n        this.isDisable1 = false;\r\n        console.log(\"1解开2不能用\");\r\n        this.input1 = \"\";\r\n      } else {\r\n        this.isDisable = false;\r\n        this.isDisable1 = true;\r\n        console.log(\"2解开1不能用\");\r\n        this.input = \"\";\r\n      }\r\n    },\r\n\r\n    show(type, row) {\r\n      this.dialogVisible = true;\r\n      this.type = type;\r\n      if (this.type == 2) {\r\n        this.tittle = \"编辑\";\r\n        console.log(row);\r\n        this.big_name = row.big_name;\r\n        this.id = row.id;\r\n        if (row.share == 10) {\r\n          this.radio = \"10\";\r\n          this.input = row.total_profit;\r\n          this.isDisable = true;\r\n        } else if (row.share == 20) {\r\n          this.radio = \"20\";\r\n          this.isDisable1 = true;\r\n          this.input1 = row.total_profit;\r\n        } else {\r\n        }\r\n      } else {\r\n        this.tittle = \"添加\";\r\n      }\r\n    },\r\n\r\n    close() {\r\n      this.dialogVisible = false;\r\n      this.radio = \"\";\r\n      this.big_name = \"\";\r\n      this.total_profit = \"\";\r\n      this.input = \"\";\r\n      this.input1 = \"\";\r\n    },\r\n\r\n    submitForm() {\r\n      if (this.input1 == \"\") {\r\n        this.total_profit = this.input;\r\n      } else {\r\n        this.total_profit = this.input1;\r\n      }\r\n      if (this.radio == \"\") {\r\n        this.$message.error(\"请选择分润模式\");\r\n        return;\r\n      }\r\n      if (this.total_profit == \"\") {\r\n        this.$message.error(\"分润不能为空\");\r\n        return;\r\n      }\r\n\r\n      let token = sessionStorage.getItem(\"token\");\r\n      this.token = token;\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n        big_name: this.big_name,\r\n        share: this.radio,\r\n        total_profit: this.total_profit,\r\n      };\r\n      fenrunset(params).then((res) => {\r\n        if (res.data.code == 200) {\r\n          this.$message.success(\"新增成功\");\r\n          this.$parent.getUserList();\r\n          this.close();\r\n          this.isDisable = false;\r\n        } else {\r\n          this.$message.error(res.data.msg);\r\n          this.$parent.getUserList();\r\n          this.close();\r\n          this.isDisable = false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.xian {\r\n  margin-left: -75px !important;\r\n}\r\n</style>\r\n"]}]}