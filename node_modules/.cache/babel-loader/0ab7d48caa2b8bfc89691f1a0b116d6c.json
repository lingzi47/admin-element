;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,QAAA;AACA,KAvDA;AAwDA,IAAA,KAxDA,mBAwDA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,KA1DA;AA2DA,IAAA,MA3DA,oBA2DA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KA9DA;AA+DA,IAAA,WA/DA,yBA+DA;AACA,WAAA,MAAA;AACA,KAjEA;AAkEA,IAAA,UAlEA,wBAkEA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KADA;AAAA;AAAA;AAAA;;AAEA,kBAAA,KAFA,GAEA,cAAA,CAAA,OAAA,CAAA,OAAA,CAFA;AAAA;AAAA,yBAIA,KAAA,CAAA;AACA,oBAAA,UAAA,EAAA,MAAA,CAAA,QAAA,CAAA,UADA;AAEA,oBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA,QAFA;AAGA,oBAAA,EAAA,EAAA,MAAA,CAAA;AAHA,mBAAA,CAJA;;AAAA;AAIA,kBAAA,GAJA;;AAUA,sBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;;AAZA;AAcA,kBAAA,MAAA,CAAA,OAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,KAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAiBA;AApFA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"AddDialog\">\r\n    <el-dialog\r\n      title=\"pk值设置\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"700px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form :inline=\"true\" :model=\"form\" ref=\"form\">\r\n        <el-form-item style=\"float: right\">\r\n          <el-button\r\n            v-if=\"checkPermission('useradd')\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-plus\"\r\n            @click=\"addData\"\r\n            >新增</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n      <page-table\r\n        ref=\"dataTable\"\r\n        :data=\"goodsList\"\r\n        @changeCurrentPage=\"changeCurrent\"\r\n        :header-cell-style=\"{ textAlign: 'center' }\"\r\n        :cell-style=\"{ textAlign: 'center' }\"\r\n      >\r\n        <el-table-column\r\n          prop=\"sku\"\r\n          fixed\r\n          label=\"商品规格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"price\"\r\n          fixed\r\n          label=\"价格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"new_price\"\r\n          fixed\r\n          label=\"原价格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"rebater\"\r\n          fixed\r\n          label=\"推广佣金\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n          v-if=\"checkPermission(['useredit', 'userdelete'])\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-link\r\n              style=\"margin-right: 10px\"\r\n              type=\"primary\"\r\n              @click=\"editData(2, scope.row)\"\r\n              >修改</el-link\r\n            >\r\n            <el-link\r\n              type=\"danger\"\r\n              style=\"margin-right: 10px\"\r\n              @click=\"deleteData(scope.row)\"\r\n              >删除</el-link\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </page-table>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <div class=\"AddDialog\">\r\n      <el-dialog\r\n        :title=\"type == 1 ? '新增' : '编辑'\"\r\n        :visible.sync=\"dialogVisible1\"\r\n        width=\"900px\"\r\n        :close-on-click-modal=\"false\"\r\n        @close=\"close1\"\r\n      >\r\n        <el-form\r\n          :model=\"goodsForm\"\r\n          ref=\"goodsForm\"\r\n          :rules=\"rules\"\r\n          label-width=\"auto\"\r\n        >\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"现价格\" prop=\"new_price\">\r\n              <el-input\r\n                v-model=\"goodsForm.new_price\"\r\n                placeholder=\"请输入现价格\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"原价格\" prop=\"old_price\">\r\n              <el-input\r\n                v-model=\"goodsForm.old_price\"\r\n                placeholder=\"请输入原价格\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"推广佣金\" prop=\"rebater\">\r\n              <el-input\r\n                v-model=\"goodsForm.rebater\"\r\n                placeholder=\"请输入推广佣金\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"总销售\" prop=\"sale_num\">\r\n              <el-input\r\n                v-model=\"goodsForm.sale_num\"\r\n                placeholder=\"请输入总销售\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"保障\" prop=\"guarantee\">\r\n              <el-input\r\n                v-model=\"goodsForm.guarantee\"\r\n                placeholder=\"请输入保障\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"标签\" prop=\"label\">\r\n              <el-input\r\n                v-model=\"goodsForm.label\"\r\n                placeholder=\"请输入标签\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"submitForm1\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { checkPermission } from \"@/utils/permissions\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\n\r\nimport { skulist, skuadd, skudel, skuedit } from \"@/api/shop\";\r\nexport default {\r\n  components: {\r\n    pageTable,\r\n  },\r\n  data() {\r\n    return {\r\n      goodsForm: {\r\n        goods_name: \"\",\r\n        have_num: \"\",\r\n        brief: \"\",\r\n        label: \"\",\r\n        guarantee: \"\",\r\n        sale_num: \"\",\r\n        rebater: \"\",\r\n        old_price: \"\",\r\n        new_price: \"\",\r\n        type_id: \"\",\r\n        content: \"\",\r\n        shop_type: 1,\r\n        goods_img: \"\",\r\n      },\r\n      id: \"\",\r\n      token: \"\",\r\n      goodsList: [],\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.shopList(); //获取列表\r\n  },\r\n  mounted: function () {},\r\n  methods: {\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.page = page;\r\n      this.page.limit = size;\r\n      this.shopList();\r\n    },\r\n    addData() {\r\n      // 1:新增，2:编辑\r\n      this.type = 1;\r\n      this.dialogVisible1 = true;\r\n    },\r\n    editData(type, row) {\r\n      this.type = 2;\r\n      console.log(row);\r\n      this.dialogVisible1 = true;\r\n    },\r\n    //商品列表\r\n    async shopList() {\r\n      let res = await skulist({\r\n        sku_id: this.id,\r\n      });\r\n      if (res.status == 200) {\r\n        console.log(\"sku列表\", res.data);\r\n        // this.page.total = res.count;\r\n        this.goodsList = res.data;\r\n        // this.$refs.dataTable.setPageInfo({\r\n        //   total: res.count,\r\n        // });\r\n      }\r\n    },\r\n\r\n    //删除\r\n    deleteData(row) {\r\n      this.$confirm(\"是否删除？\", \"提示\", {\r\n        type: \"warning\",\r\n      })\r\n        .then(async () => {\r\n          console.log(row.id);\r\n          let res = await skudel({\r\n            id: row.id,\r\n          });\r\n          if (res.status == 200) {\r\n            this.shopList();\r\n            this.$message.success(\"删除成功\");\r\n          }\r\n        })\r\n        .catch(() => {});\r\n    },\r\n\r\n    show(row) {\r\n      this.id = row.id;\r\n      this.dialogVisible = true;\r\n      this.shopList();\r\n    },\r\n    close() {\r\n      this.dialogVisible = false;\r\n    },\r\n    close1() {\r\n      this.dialogVisible1 = false;\r\n      this.dialogVisible = true;\r\n    },\r\n    submitForm1() {\r\n      this.close1();\r\n    },\r\n    submitForm() {\r\n      this.$refs.ruleForm.validate(async (valid) => {\r\n        if (valid) {\r\n          let token = sessionStorage.getItem(\"token\");\r\n\r\n          let res = await pkset({\r\n            full_price: this.ruleForm.full_price,\r\n            multiple: this.ruleForm.multiple,\r\n            id: this.id,\r\n          });\r\n\r\n          if (res.data.code == 200) {\r\n            this.$message.success(\"操作成功\");\r\n          }\r\n        }\r\n        this.$parent.shopList();\r\n        this.close();\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/shop/special/goods/components"}]}