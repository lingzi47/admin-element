A,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,0BAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,wBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,wBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA,qBALA;AAMA;;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,kBAAA,MAAA,CAAA,KAAA;;AA5DA;AAAA;;AAAA;AAAA,mDA8DA,KA9DA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAiEA;AAzHA;AArCA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-dialog\n      class=\"AddDialog\"\n      :title=\"type == 1 ? '新增' : '编辑'\"\n      :visible.sync=\"dialogVisible\"\n      width=\"800px\"\n      hegiht=\"1000px\"\n      :close-on-click-modal=\"false\"\n      @close=\"close\"\n    >\n      <el-form\n        :model=\"ruleForm\"\n        ref=\"ruleForm\"\n        :rules=\"rules\"\n        label-width=\"auto\"\n      >\n        <el-input type=\"hidden\" v-model=\"ruleForm.id\"></el-input>\n        <el-row :gutter=\"20\">\n          <el-col :span=\"12\">\n            <el-form-item label=\"权限名称\" prop=\"m_name\">\n              <el-input\n                v-model=\"ruleForm.m_name\"\n                placeholder=\"请输入权限名称\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-form-item\n              label=\"类型\"\n              prop=\"type\"\n              :rules=\"[\n                { required: true, message: '类型不能为空', trigger: 'blur' },\n              ]\"\n            >\n              <el-select\n                v-model=\"ruleForm.type\"\n                clearable\n                placeholder=\"请选择角色\"\n              >\n                <el-option label=\"菜单\" :value=\"1\"></el-option>\n                <el-option label=\"按钮\" :value=\"2\"></el-option>\n              </el-select> </el-form-item\n          ></el-col>\n          <el-col :span=\"12\">\n            <el-form-item label=\"路由地址\" prop=\"route\">\n              <el-input\n                v-model=\"ruleForm.route\"\n                placeholder=\"请输入路由地址\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-form-item label=\"排序\" prop=\"sort\">\n              <el-input\n                v-model=\"ruleForm.sort\"\n                placeholder=\"请输入排序\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-form-item label=\"是否跳转界面\" prop=\"target\">\n              <el-radio-group v-model=\"ruleForm.target\">\n                <el-radio label=\"1\">是</el-radio>\n                <el-radio label=\"2\">否</el-radio>\n              </el-radio-group>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { addMenu, menuShow, editMenu } from \"@/request/api\";\nexport default {\n  name: \"AddDialog\",\n  components: {},\n  data() {\n    return {\n      id: \"\",\n      token: \"\",\n      type: \"\", //1新增，2编辑\n      dialogVisible: false,\n      ruleForm: {\n        id: \"\",\n        route: \"\",\n        target: \"\",\n        m_name: \"\",\n        sort: \"\",\n        type: \"\",\n      },\n      rules: {\n        m_name: [\n          { required: true, message: \"权限名称不能为空\", trigger: \"blur\" },\n        ],\n        target: [\n          { required: true, message: \"请选择是否跳转新页面\", trigger: \"blur\" },\n        ],\n        route: [\n          { required: true, message: \"路由地址不能为空\", trigger: \"blur\" },\n        ],\n        sort: [{ required: true, message: \"序号不能为空\", trigger: \"blur\" }],\n      },\n    };\n  },\n  created: function () {\n    //console.log(this.type);\n\n    this.getUserEdit();\n  },\n  mounted: function () {},\n  methods: {\n    //获取修改的信息\n    getUserEdit() {},\n    show(type, row) {\n      this.type = type;\n      this.dialogVisible = true;\n      if (type == 1) {\n        this.ruleForm = {\n          id: \"\",\n          route: \"\",\n          target: \"\",\n          m_name: \"\",\n          sort: \"\",\n          type: \"\",\n        };\n      } else if (type == 3) {\n        this.ruleForm = row;\n        let id = this.ruleForm.id;\n        this.id = id;\n        let params = {\n          token: sessionStorage.getItem(\"token\"),\n        };\n        menuShow(params, id).then((res) => {\n          //console.log(res.data.data);\n          this.pid = res.data.data.pid;\n          //console.log(this.pid);\n          this.ruleForm = {\n            id: \"\",\n            route: \"\",\n            target: \"\",\n            m_name: \"\",\n            sort: \"\",\n            type: \"\",\n          };\n        });\n      } else {\n        this.ruleForm = row;\n        //console.log(this.ruleForm.id);\n        let id = this.ruleForm.id;\n        this.id = id;\n        let params = {\n          token: sessionStorage.getItem(\"token\"),\n        };\n        menuShow(params, id).then((res) => {\n          //console.log(res.data.data);\n          this.ruleForm = res.data.data;\n        });\n      }\n    },\n    close() {\n      this.dialogVisible = false;\n      this.$nextTick(() => {\n        this.$refs.ruleForm.clearValidate(); //关闭清空校验规则\n      });\n    },\n    submitForm() {\n      this.$refs.ruleForm.validate(async (valid) => {\n        if (valid) {\n          if (this.type == 1) {\n            let token = sessionStorage.getItem(\"token\");\n            this.token = token;\n            let params = {\n              pid: 0,\n              token: this.token,\n              route: this.ruleForm.route,\n              m_name: this.ruleForm.m_name,\n              sort: this.ruleForm.sort,\n              target: this.ruleForm.target,\n              type: this.ruleForm.type,\n            };\n            addMenu(params).then((res) => {\n              if (res.data.code == 200) {\n                this.$message.success(\"新增成功\");\n                // this.$router.go(0);\n              }\n            });\n          } else if ((this.type = 3)) {\n            //console.log(this.pid);\n            let token = sessionStorage.getItem(\"token\");\n            this.token = token;\n            let params = {\n              token: this.token,\n              pid: this.id,\n              route: this.ruleForm.route,\n              m_name: this.ruleForm.m_name,\n              sort: this.ruleForm.sort,\n              target: this.ruleForm.target,\n              type: this.ruleForm.type,\n            };\n            addMenu(params).then((res) => {\n              if (res.data.code == 200) {\n                this.$message.success(\"新增成功\");\n                // this.$router.go(0);\n              }\n            });\n          } else {\n            let token = sessionStorage.getItem(\"token\");\n            this.token = token;\n            let id = this.id;\n            let params = {\n              token: this.token,\n              route: this.ruleForm.route,\n              m_name: this.ruleForm.m_name,\n              sort: this.ruleForm.sort,\n              target: this.ruleForm.target,\n              type: this.ruleForm.type,\n            };\n\n            editMenu(params, id).then((res) => {\n              if (res.data.code == 200) {\n                this.$message.success(\"编辑成功\");\n                this.$router.go(0);\n              }\n            });\n          }\n          this.$parent.getUserList();\n          this.close();\n        } else {\n          return false;\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/views/user/powermanage/powerList/components"}]}