ams = {\r\n            token: sessionStorage.getItem(\"token\"),\r\n            num: this.ruleForm.num,\r\n            coupon_id: this.ruleForm.coupon_id,\r\n          };\r\n          coupontaskadd(params).then((res) => {\r\n            if (res.data.code == 200) {\r\n              this.$message.success(\"新增成功\");\r\n              this.ruleForm.coupon_id = \"\";\r\n              this.ruleForm.num = \"\";\r\n            }\r\n          });\r\n\r\n          this.close();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}