atus == 200) {\r\n        this.shoporder = res.data.data;\r\n      }\r\n    },\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getUserList();\r\n    },\r\n\r\n    open() {\r\n      this.$confirm(\"退款此订单\", \"提示\", {\r\n        type: \"error\",\r\n      })\r\n        .then(async () => {\r\n          let rowData = type == 1 ? this.tableSelectList[0] : row;\r\n          let res = await shopTypedel({\r\n            id: rowData.id,\r\n          });\r\n          if (res.status == 200) {\r\n            this.getlist();\r\n            this.$message.success(\"退款成功\");\r\n          }\r\n        })\r\n        .catch(() => {});\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.census {\r\n  padding-left: 80px;\r\n}\r\n</style>\r\n"]}]}