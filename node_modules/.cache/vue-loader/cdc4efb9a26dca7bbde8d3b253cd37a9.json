  <el-row :gutter=\"20\" style=\"margin-let: 1500px\">\r\n      <el-col :lg=\"12\" :md=\"24\">\r\n        <span class=\"tiao\">参加活动人数统计 ：</span>\r\n        <table border=\"1\">\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">参与活动总人数</th>\r\n            <th style=\"width: 200px\" align=\"center\">今日参加活动人数</th>\r\n            <th style=\"width: 200px\" align=\"center\">7日参加活动人数</th>\r\n          </tr>\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.userall }} 人（次）\r\n            </th>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.userday }} 人（次）\r\n            </th>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.user7 }} 人（次）\r\n            </th>\r\n          </tr>\r\n        </table>\r\n        <p style=\"padding: 10px\"></p>\r\n\r\n        <span class=\"tiao\">渠道人数划分 ：</span>\r\n        <table border=\"1\">\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">ios端参加活动人数</th>\r\n            <th style=\"width: 200px\" align=\"center\">安卓端参加活动总人数</th>\r\n          </tr>\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">{{ list.ios }} 人（次）</th>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.android }} 人（次）\r\n            </th>\r\n          </tr>\r\n        </table>\r\n        <p style=\"padding: 10px\"></p>\r\n        <span class=\"tiao\">时间区间选择 ：</span>\r\n        <el-date-picker\r\n          v-model=\"time\"\r\n          type=\"datetimerange\"\r\n          value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n          range-separator=\"至\"\r\n          start-placeholder=\"开始日期\"\r\n          end-placeholder=\"结束日期\"\r\n        >\r\n        </el-date-picker>\r\n        <el-button\r\n          type=\"info\"\r\n          round\r\n          style=\"padding: 8px 20px; margin-left: 10px\"\r\n          @click=\"admin\"\r\n          >确认</el-button\r\n        >\r\n        <p style=\"padding: 10px\"></p>\r\n        <table border=\"1\">\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">参加活动人数</th>\r\n            <th style=\"width: 200px\" align=\"center\">ios端参加活动人数</th>\r\n            <th style=\"width: 200px\" align=\"center\">安卓端参加活动总人数</th>\r\n          </tr>\r\n          <tr>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.usertimeall }} 人（次）\r\n            </th>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.usertimeios }} 人（次）\r\n            </th>\r\n            <th style=\"width: 200px\" align=\"center\">\r\n              {{ list.usertimeaz }} 人（次）\r\n            </th>\r\n          </tr>\r\n        </table>\r\n      </el-col>\r\n      <el-col :lg=\"12\" :md=\"24\">\r\n        <div\r\n          class=\"chart\"\r\n          id=\"barScore\"\r\n          style=\"background-color: #fff; height: 500px\"\r\n        ></div>\r\n      </el-col>\r\n    </el-row>\r\n    <p style=\"padding: 10px\"></p>\r\n    <span class=\"tiao\">抽奖奖品中奖人数统计</span>\r\n    <p style=\"padding: 10px\"></p>\r\n    <span class=\"tiao\">时间区间选择 ：</span>\r\n    <el-date-picker\r\n      v-model=\"timee\"\r\n      type=\"datetimerange\"\r\n      value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n      range-separator=\"至\"\r\n      start-placeholder=\"开始日期\"\r\n      end-placeholder=\"结束日期\"\r\n    >\r\n    </el-date-picker>\r\n    <el-button\r\n      type=\"info\"\r\n      round\r\n      style=\"padding: 8px 20px; margin-left: 10px\"\r\n      @click=\"admin1\"\r\n      >确认</el-button\r\n    >\r\n    <p style=\"padding: 10px\"></p>\r\n    <el-table\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 30%\"\r\n      :header-cell-style=\"{ 'text-align': 'center' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n    >\r\n      <el-table-column prop=\"name\" label=\"奖品\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column prop=\"num\" label=\"获奖人数\" align=\"center\">\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport * as echarts from \"echarts\";\r\nimport { getechartsData } from \"@/api/echarts\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      time: \"\",\r\n      timee: \"\",\r\n      list: [],\r\n      sales: [],\r\n      //   lineScoreChart: null, //折线图\r\n      barScoreChart: null, //柱状图\r\n      month: [], //月份\r\n      subject: [], //人\r\n      mSalesVolume: [],\r\n      mSalesAmount: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    let self = this;\r\n    window.onresize = function () {\r\n      //   if (self.lineScoreChart) self.lineScoreChart.resize();\r\n      if (self.barScoreChart) self.barScoreChart.resize();\r\n    };\r\n  },\r\n  created() {\r\n    this.getechartsData(); //获取图标数据\r\n    axios.post(\"https://yjwl01.cn/api/Calculation/luck\").then((res) => {\r\n      this.list = res.data.data;\r\n      // console.log(res.data.content);\r\n\r\n      // this.count = res.data.pageinfo.count;\r\n      this.tableData = res.data.data.luckinfo;\r\n    });\r\n  },\r\n  methods: {\r\n    setBarScore() {\r\n      //设置柱状图\r\n      this.barScoreChart = echarts.init(document.getElementById(\"barScore\"));\r\n      let series = [];\r\n      this.mSalesAmount.map((item, index) => {\r\n        series.push({\r\n          type: \"bar\",\r\n          name: this.subject[index],\r\n\r\n          emphasis: {\r\n            focus: \"series\",\r\n          },\r\n          data: item,\r\n        });\r\n      });\r\n      console.log(series);\r\n      let subject = JSON.parse(JSON.stringify(this.subject));\r\n      console.log(subject);\r\n\r\n      let option = {\r\n        title: {\r\n          text: \"首次参与活动用户统计\",\r\n          left: \"center\",\r\n        },\r\n        tooltip: {\r\n          trigger: \"axis\",\r\n          axisPointer: {\r\n            type: \"shadow\",\r\n          },\r\n        },\r\n        grid: {\r\n          bottom: 70,\r\n          left: 70,\r\n          right: 30,\r\n        },\r\n        legend: {\r\n          bottom: 10,\r\n          data: subject,\r\n        },\r\n        xAxis: {\r\n          data: this.month,\r\n          type: \"category\",\r\n        },\r\n        yAxis: [\r\n          {\r\n            type: \"value\",\r\n          },\r\n        ],\r\n        series: series,\r\n      };\r\n      this.barScoreChart.setOption(option);\r\n    },\r\n    async getechartsData() {\r\n      let res = await getechartsData();\r\n      if (res.status == 200) {\r\n        // console.log(JSON.parse(JSON.stringify(res.data)));\r\n        let data = res.data;\r\n        this.sales = res.data;\r\n        console.log(this.sales);\r\n        let month = [];\r\n        let subject = [];\r\n        let mSalesVolume = [[], [], [], []];\r\n        let mSalesAmount = [[], [], [], []];\r\n        data.map((d, i) => {\r\n          month.push(d.date);\r\n          d.personnel.map((p, j) => {\r\n            if (subject.indexOf(p.name) == -1) subject.push(p.name);\r\n            mSalesVolume[j].push(p.salesVolume);\r\n            mSalesAmount[j].push(p.salesAmount);\r\n          });\r\n        });\r\n        this.month = month;\r\n        console.log(this.month);\r\n        this.subject = subject;\r\n        console.log(this.subject);\r\n        this.mSalesVolume = mSalesVolume;\r\n        console.log(this.mSalesVolume);\r\n        this.mSalesAmount = mSalesAmount;\r\n        console.log(this.mSalesAmount);\r\n        // this.setLineScore();\r\n        this.setBarScore();\r\n      }\r\n    },\r\n    admin() {\r\n      this.time1 = this.time[0];\r\n      this.time2 = this.time[1];\r\n      if (this.time1 == undefined) {\r\n        this.ime1 = \"\";\r\n        this.time2 = \"\";\r\n        var formdata = new FormData();\r\n        formdata.append(\"time1\", this.time1);\r\n        formdata.append(\"time2\", this.time2);\r\n        axios\r\n          .post(\"https://yjwl01.cn/api/Calculation/luck\", formdata)\r\n          .then((res) => {\r\n            this.list = res.data.data;\r\n          });\r\n      } else {\r\n        var formdata = new FormData();\r\n        formdata.append(\"time1\", this.time1);\r\n        formdata.append(\"time2\", this.time2);\r\n        axios\r\n          .post(\"https://yjwl01.cn/api/Calculation/luck\", formdata)\r\n          .then((res) => {\r\n            this.list = res.data.data;\r\n          });\r\n      }\r\n    },\r\n    admin1() {\r\n      this.lucktime1 = this.timee[0];\r\n      this.lucktime2 = this.timee[1];\r\n      if (this.lucktime1 == undefined) {\r\n        this.lucktime1 = \"\";\r\n        this.lucktime2 = \"\";\r\n        var formdata = new FormData();\r\n        formdata.append(\"lucktime1\", this.lucktime1);\r\n        formdata.append(\"lucktime2\", this.lucktime2);\r\n        axios\r\n          .post(\"https://yjwl01.cn/api/Calculation/luck\", formdata)\r\n          .then((res) => {\r\n            this.tableData = res.data.data.luckinfo;\r\n          });\r\n      } else {\r\n        var formdata = new FormData();\r\n        formdata.append(\"lucktime1\", this.lucktime1);\r\n        formdata.append(\"lucktime2\", this.lucktime2);\r\n        axios\r\n          .post(\"https://yjwl01.cn/api/Calculation/luck\", formdata)\r\n          .then((res) => {\r\n            this.tableData = res.data.data.luckinfo;\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.echarts {\r\n  .explain {\r\n    margin-bottom: 10px;\r\n\r\n    a {\r\n      color: #409eff;\r\n    }\r\n\r\n    a:hover {\r\n      color: #66b1ff;\r\n      text-decoration: underline;\r\n    }\r\n  }\r\n\r\n  .chart {\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n.tiao {\r\n  color: rgb(151, 151, 151);\r\n  font-size: 14px;\r\n  padding: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n.btn {\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\ntable {\r\n  border-collapse: collapse;\r\n\r\n  text-align: center;\r\n}\r\ntable td,\r\ntable th {\r\n  border: 1px solid #cad9ea;\r\n  color: #666;\r\n  height: 30px;\r\n  font-size: 12px;\r\n}\r\ntable thead th {\r\n  background-color: #cce8eb;\r\n  width: 100px;\r\n}\r\ntable tr:nth-child(odd) {\r\n  background: #fff;\r\n}\r\ntable tr:nth-child(even) {\r\n  background: #f5fafa;\r\n}\r\n</style>"]}]}