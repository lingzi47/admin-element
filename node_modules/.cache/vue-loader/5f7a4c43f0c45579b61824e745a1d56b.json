 option = {\n        xAxis: {\n          data: this.xData\n        },\n        yAxis: [\n          {\n            type: \"value\",\n            splitLine: false\n          }\n        ],\n        textStyle: {\n          //图例中文字的样式\n          color: \"#F8F8FF\",\n          fontSize: 16\n        },\n        series: [\n          {\n            data: this.yData,\n            type: \"line\", // 类型设置为折线图\n            smooth: true,\n            areaStyle: {\n              normal: {\n                color: {\n                  x: 0,\n                  y: 0,\n                  x2: 0,\n                  y2: 1,\n                  colorStops: [\n                    {\n                      offset: 0,\n                      color: \"#7B68EE\" // 0% 处的颜色\n                    },\n                    {\n                      offset: 0.7,\n                      color: \"rgba(220,56,129,0)\" // 100% 处的颜色\n                    }\n                  ],\n                  globalCoord: false // 缺省为 false\n                }\n              }\n            }\n          }\n        ]\n      };\n      this.myChart = echarts.init(document.getElementById(\"mychart\"));\n      this.myChart.setOption(option);\n      //随着屏幕大小调节图表\n      window.addEventListener(\"resize\", () => {\n        this.myChart.resize();\n      });\n    }\n  }\n};\n</script>\n\n"]}]}