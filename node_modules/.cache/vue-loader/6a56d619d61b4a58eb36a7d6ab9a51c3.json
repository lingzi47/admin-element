        label-width=\"auto\"\r\n      >\r\n        <el-row :gutter=\"15\">\r\n          <el-col :span=\"20\">\r\n            <el-form-item label=\"点位位置\" prop=\"value1\">\r\n              <el-cascader\r\n                style=\"width: 250px\"\r\n                v-model=\"ruleForm.value1\"\r\n                :options=\"areaArr\"\r\n                :props=\"{ value: 'name', label: 'name' }\"\r\n                placeholder=\"请选择省市区\"\r\n                filterable\r\n                @change=\"change\"\r\n              ></el-cascader>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"点位详细位置\" prop=\"details\">\r\n              <el-input\r\n                v-model=\"ruleForm.details\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入药房详细位置\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"点位来源\" prop=\"type\">\r\n              <el-select\r\n                v-model=\"ruleForm.type\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px\"\r\n                @change=\"change1\"\r\n              >\r\n                <el-option label=\"全部\" value=\"\"></el-option>\r\n                <el-option label=\"公司\" value=\"10\"></el-option>\r\n                <el-option label=\"渠道商\" value=\"20\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <el-form-item label=\"点位使用年限\" prop=\"year\">\r\n              <el-input\r\n                v-model=\"ruleForm.year\"\r\n                :disabled=\"isDisable\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入点位使用年限\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"2\">\r\n            <el-form-item class=\"xian\">\r\n              <el-checkbox v-model=\"check\" @change=\"checked\"\r\n                >无期限</el-checkbox\r\n              >\r\n            </el-form-item></el-col\r\n          >\r\n          <el-col :span=\"24\">\r\n            <el-form-item\r\n              label=\"渠道商id:\"\r\n              prop=\"position_user\"\r\n              v-if=\"this.ruleForm.type == 20\"\r\n            >\r\n              <el-input\r\n                v-model=\"ruleForm.position_user\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入渠道商id\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { positionadd, positionedit, goodsedit } from \"@/request/api\";\r\nimport { areaListData } from \"@/utils/area\";\r\nexport default {\r\n  name: \"AddDialog\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      check: false,\r\n      token: \"\",\r\n      isDisable: false,\r\n      tittle: \"\",\r\n      form: {\r\n        value1: \"\",\r\n        value2: \"\",\r\n        value3: \"\",\r\n      },\r\n      province: \"\",\r\n      type: \"\",\r\n      city: \"\",\r\n      id: \"\",\r\n      area: \"\",\r\n      type: \"\", //1新增，2编辑\r\n      dialogVisible: false,\r\n      ruleForm: {\r\n        value1: \"\",\r\n        type: \"\",\r\n        year: \"\",\r\n        position_user: \"\",\r\n        details: \"\",\r\n      },\r\n      rules: {\r\n        year: [{ required: true, message: \"请输入使用年限\", trigger: \"blur\" }],\r\n        value1: [{ required: true, message: \"请选择省市区\", trigger: \"blur\" }],\r\n\r\n        details: [\r\n          { required: true, message: \"请输入详情信息\", trigger: \"blur\" },\r\n        ],\r\n        position_user: [\r\n          { required: true, message: \"请输入渠道商id\", trigger: \"blur\" },\r\n        ],\r\n        type: [{ required: true, message: \"请选择点位来源\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.setData(areaListData());\r\n    this.areaArr = areaListData();\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    setData(data) {\r\n      data.map((item) => {\r\n        item[\"value\"] = item.code;\r\n        item[\"label\"] = item.name;\r\n        if (item.children) {\r\n          this.setData(item.children);\r\n        }\r\n      });\r\n    },\r\n    change1(val) {\r\n      console.log(val);\r\n      if (val == 10) {\r\n        this.ruleForm.year = 0;\r\n        console.log(this.ruleForm.year);\r\n      } else {\r\n        console.log(this.ruleForm.year);\r\n      }\r\n    },\r\n    checked(events) {\r\n      // console.log(events);\r\n      this.events = events;\r\n      if (events == true) {\r\n        if (this.ruleForm.type == 10) {\r\n          this.isDisable = true;\r\n          this.ruleForm.year = 0;\r\n        } else {\r\n          this.isDisable = true;\r\n          this.ruleForm.year = 999;\r\n        }\r\n      } else {\r\n        this.isDisable = false;\r\n        this.ruleForm.year = \"\";\r\n      }\r\n      //\r\n      // console.log(\"被禁言\");\r\n      // this.ruleForm.uid = 0;\r\n    },\r\n    change(data) {\r\n      console.log(data);\r\n      this.province = data[0];\r\n      this.city = data[1];\r\n      this.area = data[2];\r\n    },\r\n    show(type, row) {\r\n      this.dialogVisible = true;\r\n      console.log(type);\r\n      this.type = type;\r\n      if (this.type == 2) {\r\n        this.tittle = \"编辑\";\r\n        console.log(row);\r\n        this.id = row.id;\r\n\r\n        this.ruleForm.type = row.type;\r\n        this.ruleForm.details = row.details;\r\n        // this.ruleForm.value1 = \"\";\r\n        if (row.year == 999) {\r\n          this.isDisable = true;\r\n          this.ruleForm.year = row.year;\r\n          this.ruleForm.position_user = row.position_user;\r\n          this.check = true;\r\n          console.log(\"被选中\");\r\n        } else {\r\n          console.log(111);\r\n\r\n          this.ruleForm.year = row.year;\r\n        }\r\n        let arr1 = [];\r\n        arr1.push(row.province);\r\n        arr1.push(row.city);\r\n        arr1.push(row.area);\r\n\r\n        console.log(arr1);\r\n        this.ruleForm.value1 = arr1;\r\n      } else {\r\n        this.tittle = \"添加\";\r\n      }\r\n    },\r\n\r\n    close() {\r\n      this.dialogVisible = false;\r\n      this.ruleForm.type = \"\";\r\n      this.ruleForm.position_user = \"\";\r\n      this.ruleForm.year = \"\";\r\n\r\n      this.ruleForm.details = \"\";\r\n      this.isDisable = false;\r\n      this.check = false;\r\n      this.ruleForm.value1 = \"\";\r\n    },\r\n\r\n    submitForm() {\r\n      if (this.type == 1) {\r\n        this.$refs.ruleForm.validate(async (valid) => {\r\n          if (valid) {\r\n            let token = sessionStorage.getItem(\"token\");\r\n            this.token = token;\r\n            let params = {\r\n              position_user: this.ruleForm.position_user,\r\n              type: this.ruleForm.type,\r\n              year: this.ruleForm.year,\r\n              token: sessionStorage.getItem(\"token\"),\r\n              details: this.ruleForm.details,\r\n              province: this.province,\r\n              city: this.city,\r\n              area: this.area,\r\n            };\r\n            positionadd(params).then((res) => {\r\n              if (res.data.code == 200) {\r\n                this.$message.success(\"新增成功\");\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              }\r\n            });\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      } else {\r\n        this.$refs.ruleForm.validate(async (valid) => {\r\n          if (valid) {\r\n            let token = sessionStorage.getItem(\"token\");\r\n            this.token = token;\r\n            let params = {\r\n              position_user: this.ruleForm.position_user,\r\n              type: this.ruleForm.type,\r\n              year: this.ruleForm.year,\r\n              token: sessionStorage.getItem(\"token\"),\r\n\r\n              details: this.ruleForm.details,\r\n              province: this.province,\r\n              city: this.city,\r\n              area: this.area,\r\n              id: this.id,\r\n            };\r\n            positionedit(params).then((res) => {\r\n              if (res.data.code == 200) {\r\n                this.$message.success(\"新增成功\");\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              } else {\r\n                this.$message.error(res.data.msg);\r\n                this.$parent.getUserList();\r\n                this.close();\r\n                this.isDisable = false;\r\n              }\r\n            });\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.xian {\r\n  margin-left: -75px !important;\r\n}\r\n</style>\r\n"]}]}