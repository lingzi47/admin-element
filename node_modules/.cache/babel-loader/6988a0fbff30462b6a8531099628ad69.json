;AAAA;AAAA;AAAA;AAQA,KA3CA;AA6CA;AACA,IAAA,UA9CA,sBA8CA,GA9CA,EA8CA;AAAA;;AACA,WAAA,QAAA,CAAA,OAAA,EAAA,IAAA,EAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA,EAGA,IAHA,uEAGA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA,EADA;AAEA,kBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA;AAFA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAKA,oBAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAHA,IAaA,KAbA,CAaA,YAAA,CAAA,CAbA;AAcA,KA7DA;AA+DA,IAAA,IA/DA,gBA+DA,GA/DA,EA+DA;AACA,WAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AAEA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,QAAA;AACA,WAAA,YAAA;AACA,KArEA;AAsEA,IAAA,KAtEA,mBAsEA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,KAxEA;AAyEA,IAAA,MAzEA,oBAyEA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KA5EA;AA6EA,IAAA,WA7EA,yBA6EA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,IAAA,IAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,OAAA,CAAA;AACA,kBAAA,MAAA,EAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,EAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA,KAHA;AAIA,kBAAA,EAAA,EAAA,MAAA,CAAA,SAAA,CAAA,EAJA;AAKA,kBAAA,OAAA,EAAA,MAAA,CAAA,SAAA,CAAA,OALA;AAMA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA,KANA;AAOA,kBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA;AAPA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAWA,oBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,QAAA;AACA;;AAdA;AAAA;;AAAA;AAAA;AAAA,uBAgBA,MAAA,CAAA;AACA,kBAAA,MAAA,EAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,EAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA,KAHA;AAIA,kBAAA,OAAA,EAAA,MAAA,CAAA,SAAA,CAAA,OAJA;AAKA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA,KALA;AAMA,kBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA;AANA,iBAAA,CAhBA;;AAAA;AAgBA,gBAAA,IAhBA;;AAwBA,oBAAA,IAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,QAAA;AACA;;AA3BA;AA8BA,gBAAA,MAAA,CAAA,OAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AA/BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCA;AA7GA;AAvCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"AddDialog\">\r\n    <el-dialog\r\n      title=\"sku设置\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"700px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form :inline=\"true\">\r\n        <el-form-item style=\"float: right\">\r\n          <el-button\r\n            v-if=\"checkPermission('useradd')\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-plus\"\r\n            @click=\"addData\"\r\n            >新增</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n      <page-table\r\n        ref=\"dataTable\"\r\n        :data=\"goodsList\"\r\n        @changeCurrentPage=\"changeCurrent\"\r\n        :header-cell-style=\"{ textAlign: 'center' }\"\r\n        :cell-style=\"{ textAlign: 'center' }\"\r\n      >\r\n        <el-table-column\r\n          prop=\"sku\"\r\n          fixed\r\n          label=\"商品规格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"price\"\r\n          fixed\r\n          label=\"价格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <!-- <el-table-column\r\n          prop=\"new_price\"\r\n          fixed\r\n          label=\"原价格\"\r\n          :resizable=\"false\"\r\n        ></el-table-column> -->\r\n\r\n        <el-table-column\r\n          prop=\"rebater\"\r\n          fixed\r\n          label=\"推广佣金\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n          v-if=\"checkPermission(['useredit', 'userdelete'])\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <!-- <el-link\r\n              style=\"margin-right: 10px\"\r\n              type=\"primary\"\r\n              @click=\"editData(2, scope.row)\"\r\n              >修改</el-link\r\n            > -->\r\n            <el-link\r\n              type=\"danger\"\r\n              style=\"margin-right: 10px\"\r\n              @click=\"deleteData(scope.row)\"\r\n              >删除</el-link\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </page-table>\r\n      <!-- <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div> -->\r\n    </el-dialog>\r\n\r\n    <div class=\"AddDialog\">\r\n      <el-dialog\r\n        title=\"sku编辑\"\r\n        :visible.sync=\"dialogVisible1\"\r\n        width=\"900px\"\r\n        :close-on-click-modal=\"false\"\r\n        @close=\"close1\"\r\n      >\r\n        <el-form\r\n          :model=\"goodsForm\"\r\n          ref=\"goodsForm\"\r\n          :rules=\"rules\"\r\n          label-width=\"auto\"\r\n        >\r\n          <el-col :span=\"12\" v-for=\"(item, index) in list\" :key=\"index\">\r\n            <el-form-item :label=\"item.name\" prop=\"name\">\r\n              <el-select\r\n                v-model=\"item.skunamearr.value\"\r\n                value-key=\"item.skunamearr.id\"\r\n                @change=\"getChange(index, item, $event)\"\r\n              >\r\n                <el-option\r\n                  v-for=\"(val, k) in list[index].skunamearr\"\r\n                  :key=\"k\"\r\n                  :value=\"val.id\"\r\n                  :label=\"val.value\"\r\n                />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"售价\" prop=\"price\">\r\n              <el-input\r\n                v-model=\"goodsForm.price\"\r\n                placeholder=\"请输入售价\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"推广佣金\" prop=\"rebater\">\r\n              <el-input\r\n                v-model=\"goodsForm.rebater\"\r\n                placeholder=\"请输入推广佣金\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"库存\" prop=\"count\">\r\n              <el-input\r\n                v-model=\"goodsForm.count\"\r\n                placeholder=\"请输入库存\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"submitForm1\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { checkPermission } from \"@/utils/permissions\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\n\r\nimport { skulist, skuadd, skudel, skuedit, shopskuindex } from \"@/api/shop\";\r\nexport default {\r\n  components: {\r\n    pageTable,\r\n  },\r\n  data() {\r\n    return {\r\n      list: [],\r\n      rules: {\r\n        name: [{ required: true, message: \"请选择属性\", trigger: \"blur\" }],\r\n\r\n        rebater: [\r\n          { required: true, message: \"请输入推广佣金\", trigger: \"blur\" },\r\n        ],\r\n        price: [{ required: true, message: \"请输入售价\", trigger: \"blur\" }],\r\n        count: [{ required: true, message: \"请输入库存\", trigger: \"blur\" }],\r\n      },\r\n      arr: [],\r\n      goodsForm: {\r\n        id: \"\",\r\n        count: \"\",\r\n        price: \"\",\r\n        rebater: \"\",\r\n        sku_id: \"\",\r\n        goods_id: \"\",\r\n      },\r\n      id: \"\",\r\n      index: \"\",\r\n      printerSelect: [],\r\n      token: \"\",\r\n      goodsList: [],\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      sku_id: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.shopList(); //获取列表\r\n  },\r\n  mounted: function () {},\r\n  methods: {\r\n    // 获取打印内容下拉框的选择项,保存到数组\r\n    getChange(index, item, $event) {\r\n      this.arr[index] = $event;\r\n    },\r\n\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.page = page;\r\n      this.page.limit = size;\r\n      this.shopList();\r\n    },\r\n    addData() {\r\n      // 1:新增，2:编辑\r\n      this.type = 1;\r\n      this.goodsForm = {};\r\n      this.dialogVisible1 = true;\r\n    },\r\n    editData(type, row) {\r\n      this.type = 2;\r\n      this.goodsForm = row;\r\n      this.goodsForm.id = row.id;\r\n      this.dialogVisible1 = true;\r\n    },\r\n    async shopskuindex() {\r\n      let res = await shopskuindex({\r\n        goods_id: this.id,\r\n        token: sessionStorage.getItem(\"token\"),\r\n      });\r\n      if (res.status == 200) {\r\n        this.list = res.data.data;\r\n      }\r\n    },\r\n    //商品列表\r\n    async shopList() {\r\n      let res = await skulist({\r\n        goods_id: this.id,\r\n        token: sessionStorage.getItem(\"token\"),\r\n      });\r\n      if (res.status == 200) {\r\n        this.goodsList = res.data.data;\r\n      }\r\n    },\r\n\r\n    //删除\r\n    deleteData(row) {\r\n      this.$confirm(\"是否删除？\", \"提示\", {\r\n        type: \"warning\",\r\n      })\r\n        .then(async () => {\r\n          let res = await skudel({\r\n            id: row.id,\r\n            token: sessionStorage.getItem(\"token\"),\r\n          });\r\n          if (res.status == 200) {\r\n            this.shopList();\r\n            this.$message.success(\"删除成功\");\r\n          }\r\n        })\r\n        .catch(() => {});\r\n    },\r\n\r\n    show(row) {\r\n      this.id = row.id;\r\n\r\n      this.dialogVisible = true;\r\n      this.shopList();\r\n      this.shopskuindex();\r\n    },\r\n    close() {\r\n      this.dialogVisible = false;\r\n    },\r\n    close1() {\r\n      this.dialogVisible1 = false;\r\n      this.dialogVisible = true;\r\n    },\r\n    async submitForm1() {\r\n      if (this.type == 2) {\r\n        let res = await skuedit({\r\n          sku_id: this.arr.join(\",\"),\r\n          goods_id: this.id,\r\n          price: this.goodsForm.price,\r\n          id: this.goodsForm.id,\r\n          rebater: this.goodsForm.rebater,\r\n          count: this.goodsForm.count,\r\n          token: sessionStorage.getItem(\"token\"),\r\n        });\r\n        if (res.data.code == 200) {\r\n          this.$message.success(\"操作成功\");\r\n          this.shopList();\r\n        }\r\n      } else {\r\n        let res = await skuadd({\r\n          sku_id: this.arr.join(\",\"),\r\n          goods_id: this.id,\r\n          price: this.goodsForm.price,\r\n          rebater: this.goodsForm.rebater,\r\n          count: this.goodsForm.count,\r\n          token: sessionStorage.getItem(\"token\"),\r\n        });\r\n        if (res.data.code == 200) {\r\n          this.$message.success(\"操作成功\");\r\n          this.shopList();\r\n        }\r\n      }\r\n\r\n      this.$parent.shopList();\r\n      this.close1();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/shop/special/specialgoods/components"}]}