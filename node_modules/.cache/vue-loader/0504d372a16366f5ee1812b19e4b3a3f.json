\n            <el-input\r\n              style=\"width: 180px\"\r\n              v-model=\"form.uid\"\r\n              clearable\r\n              placeholder=\"请输入用户ID\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"姓名\" prop=\"nickname\">\r\n            <el-input\r\n              style=\"width: 170px\"\r\n              v-model=\"form.nickname\"\r\n              clearable\r\n              placeholder=\"请输入收件人姓名\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item style=\"float: right\">\r\n            <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"searchinfo\"\r\n              >搜索</el-button\r\n            >\r\n            <el-button @click=\"dao\">导出</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <page-table\r\n        ref=\"dataTable\"\r\n        :data=\"shoporder\"\r\n        @changeCurrentPage=\"changeCurrent\"\r\n      >\r\n        <el-table-column\r\n          label=\"序号\"\r\n          align=\"center\"\r\n          width=\"130\"\r\n          fixed\r\n          :resizable=\"false\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ (page.page - 1) * page.limit + scope.$index + 1 }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"uid\"\r\n          label=\"购买者id\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"nickname\"\r\n          label=\"收货人\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"order_no\"\r\n          width=\"220\"\r\n          label=\"订单号\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"pay_price\"\r\n          label=\"订单金额\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"pay_time\"\r\n          label=\"支付时间\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          prop=\"sign_time\"\r\n          label=\"自提发货时间\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n        <el-table-column label=\"支付状态\" align=\"center\" :resizable=\"false\">\r\n          <template slot-scope=\"scope\">\r\n            <el-link type=\"success\" v-if=\"scope.row.order_status == 1\"\r\n              >待付款</el-link\r\n            >\r\n            <el-link type=\"danger\" v-if=\"scope.row.order_status == 2\"\r\n              >已付款</el-link\r\n            >\r\n            <el-link type=\"danger\" v-if=\"scope.row.order_status == 3\"\r\n              >已取消</el-link\r\n            >\r\n            <el-link type=\"danger\" v-if=\"scope.row.order_status == 4\"\r\n              >退款</el-link\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"120\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n          v-if=\"checkPermission(['useredit', 'userdelete'])\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-link\r\n              v-if=\"scope.row.order_status == 2\"\r\n              type=\"danger\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"fa(scope.row)\"\r\n              >自提发货</el-link\r\n            >\r\n            <el-link\r\n              v-if=\"scope.row.order_status == 2\"\r\n              type=\"danger\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"refund(scope.row)\"\r\n              >退款</el-link\r\n            >\r\n            <el-link\r\n              v-if=\"scope.row.order_status == 4\"\r\n        