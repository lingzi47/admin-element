AA,CAvBA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,eAAA,EAAA,eADA;AAEA;AACA,IAAA,YAHA,wBAGA,MAHA,EAGA;AACA,WAAA,eAAA,GAAA,MAAA;AACA,KALA;AAMA,IAAA,OANA,qBAMA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,QAAA,CAAA,KAAA,CAAA,IAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAXA;AAYA,IAAA,YAZA,0BAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,aAAA,EADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,kBAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,kBAAA,MAAA,CAAA,cAAA,GAAA,EAAA;AACA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAnBA;AAoBA,IAAA,UApBA,sBAoBA,GApBA,EAoBA;AACA,WAAA,cAAA,GAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,GAAA,IAAA,GAAA;AAAA,OAAA,CAAA;AACA,KAtBA;AAuBA,IAAA,QAvBA,sBAuBA;AACA,UAAA,KAAA,eAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,eAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,eAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,UAAA;AACA;AACA;AA/BA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"menu\">\n    <div class=\"block-quote\">\n      <el-form :inline=\"true\" ref=\"form\">\n        <el-form-item label=\"一级菜单\" prop=\"menuId1\">\n          <el-select v-model=\"form.menuId1\" clearable filterable placeholder=\"请选择一级菜单\" @change=\"menuChange\">\n            <el-option v-for=\"(item) in menuLevel1\" :value=\"item.id\" :label=\"item.title\" :key=\"item.id\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"二级菜单\" prop=\"menuId2\">\n          <el-select v-model=\"form.menuId2\" clearable filterable placeholder=\"请选择二级菜单\">\n            <el-option v-for=\"(item) in showMenuLevel2\" :value=\"item.id\" :label=\"item.title\" :key=\"item.id\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item style=\"float: right;\">\n          <el-button v-if=\"checkPermission('menusearch')\" type=\"primary\" icon=\"el-icon-search\" @click=\"getMenu\">查询</el-button>\n          <el-button v-if=\"checkPermission('menuedit')\" type=\"primary\" icon=\"el-icon-edit\" @click=\"editData()\">编辑</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n    <page-table ref=\"dataTable\" :data=\"menuList\" :paging=\"false\" row-key=\"id\" @selection-change=\"getSelection\">\n      <el-table-column type=\"selection\" fixed width=\"40\" :resizable=\"false\"></el-table-column>\n      <el-table-column prop=\"id\" label=\"编号\" width=\"200\" :resizable=\"false\"></el-table-column>\n      <el-table-column prop=\"name\" label=\"菜单\" width=\"200\" :resizable=\"false\"></el-table-column>\n      <el-table-column prop=\"title\" label=\"菜单名称\" width=\"200\" :resizable=\"false\"></el-table-column>\n      <el-table-column prop=\"path\" label=\"菜单路径\" :resizable=\"false\"></el-table-column>\n    </page-table>\n\n    <!-- 编辑弹窗 -->\n    <edit-dialog ref=\"editData\"></edit-dialog>\n  </div>\n</template>\n\n<script>\nimport { checkPermission } from \"@/utils/permissions\";\nimport { getMenu, getLevelMenu } from '@/api/operation'\nimport pageTable from \"@/components/pageTable.vue\"\nimport editDialog from \"./components/editDialog.vue\"\nexport default {\n  name: \"menuPage\",\n  components: {\n    pageTable,\n    editDialog\n  },\n  data() {\n    return {\n      menuList: [],\n      tableSelectList: [],\n      form: {\n        menuId1: '',\n        menuId2: ''\n      },\n      menuLevel1: [], //一级菜单\n      menuLevel2: [],  //二级菜单\n      showMenuLevel2: []\n    }\n  },\n  created() {\n    this.getMenu();\n    this.getLevelMenu();\n  },\n  mounted() { },\n  methods: {\n    checkPermission,\n    //监听表格多选\n    getSelection(select) {\n      this.tableSelectList = select;\n    },\n    async getMenu() {\n      let res = await getMenu(this.form);\n      if (res.status == 200) {\n        this.menuList = res.data;\n      }\n    },\n    async getLevelMenu() {\n      let res = await getLevelMenu();\n      if (res.status == 200) {\n        this.menuLevel1 = res.data.menuLevel1\n        this.menuLevel2 = res.data.menuLevel2\n        this.showMenuLevel2 = []\n      }\n    },\n    menuChange(val) {\n      this.showMenuLevel2 = this.menuLevel2.filter(item => item.pid == val);\n    },\n    editData() {\n      if (this.tableSelectList.length == 1) {\n        this.$refs.editData.show(JSON.parse(JSON.stringify(this.tableSelectList[0])));\n      } else if (this.tableSelectList.length == 0) {\n        this.$message.warning(\"请选择一条数据编辑\");\n      } else {\n        this.$message.warning(\"暂不支持批量编辑\");\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"],"sourceRoot":"src/views/authority/menu"}]}