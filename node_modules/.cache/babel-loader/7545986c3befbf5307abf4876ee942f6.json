AA,IAAA,CAAA;AADA,SAAA;AAGA,OAnBA;AAoBA;AAxCA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <div style=\"margin-left: 200px\">\r\n      <el-form :inline=\"true\">\r\n        <el-form-item style=\"margin-left: 50px\">\r\n          <span style=\"font-size: 15px\">应急箱数量:</span>\r\n          {{ allbox }}台</el-form-item\r\n        >\r\n        <!-- <el-form-item style=\"margin-left: 50px\">\r\n          <span style=\"font-size: 15px\">产生收益设备:</span>\r\n          {{ isserve }}</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\">\r\n          <span style=\"font-size: 15px\">未缴纳通讯服务费设备数量:</span>\r\n          {{ noserve }}</el-form-item\r\n        >\r\n-->\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">渠道药品总收益:</span>\r\n          {{ position }}</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">药品总收益 :</span>\r\n          {{ isprice }}元</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">已提现收益:</span>\r\n          {{ with1 }}元</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">广告总收益 :</span>\r\n          {{ ad_sum }}元</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">广告可提现收益:</span>\r\n          {{ ad_canWith }}元</el-form-item\r\n        >\r\n        <el-form-item style=\"margin-left: 50px\"\r\n          ><span style=\"font-size: 15px\">已提现广告:</span>\r\n          {{ ad_with }}元</el-form-item\r\n        >\r\n      </el-form>\r\n    </div>\r\n\r\n    <page-table\r\n      ref=\"dataTable\"\r\n      :data=\"userList\"\r\n      @changeCurrentPage=\"changeCurrent\"\r\n    >\r\n      <el-table-column label=\"序号\" align=\"center\" :resizable=\"false\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{\r\n            (page.currentPage - 1) * page.pageSize + scope.$index + 1\r\n          }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"uid\"\r\n        label=\"用户id\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"price\"\r\n        label=\"提现金额\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"bank\"\r\n        label=\"银行卡号\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"tel\"\r\n        label=\"手机号\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"created_at\"\r\n        label=\"提现时间\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"ex_name\"\r\n        label=\"审核人\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"ex_time\"\r\n        label=\"审核时间\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        prop=\"type\"\r\n        label=\"审核状态\"\r\n        align=\"center\"\r\n        :resizable=\"false\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <el-link type=\"danger\" v-if=\"scope.row.ex_status == 3\"\r\n            >已拒绝</el-link\r\n          >\r\n          <el-link type=\"success\" v-if=\"scope.row.ex_status == 2\"\r\n            >已通过</el-link\r\n          >\r\n          <el-link type=\"warning\" v-if=\"scope.row.ex_status == 1\"\r\n            >待处理</el-link\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </page-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { boxWithInfo } from \"@/request/api\";\r\nimport { checkPermission } from \"@/utils/permissions\";\r\nimport { getUserList, deleteUser } from \"@/api/user\";\r\nimport { getRoles } from \"@/api/role\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\n\r\nexport default {\r\n  name: \"user\",\r\n  components: {\r\n    pageTable,\r\n  },\r\n  data() {\r\n    return {\r\n      userList: [], // 用户列表\r\n      id: \"\",\r\n      noserve: \"\",\r\n      allbox: \"\",\r\n      isprice: \"\",\r\n      isserve: \"\",\r\n      noprice: \"\",\r\n      position: \"\",\r\n      canWith: \"\",\r\n      with1: \"\",\r\n      ad_canWith: \"\",\r\n      ad_with: \"\",\r\n      ad_sum: \"\",\r\n      page: {\r\n        //分页信息\r\n        currentPage: 1, //当前页\r\n        pageSize: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.id = this.$route.query.id;\r\n\r\n    this.getUserList(); //获取用户列表\r\n  },\r\n  mounted() {},\r\n  computed: {},\r\n  methods: {\r\n    // row是我从上边函数传下来的数据，可以拿到当前选中的状态值，下边的请求是因为我要传给后端调的接口\r\n\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getUserList();\r\n    },\r\n\r\n    getUserList() {\r\n      let token = sessionStorage.getItem(\"token\");\r\n      this.token = token;\r\n      let params = {\r\n        page: this.page.currentPage,\r\n        limit: this.page.pageSize,\r\n        token: sessionStorage.getItem(\"token\"),\r\n        uid: this.id,\r\n      };\r\n      boxWithInfo(params).then((res) => {\r\n        this.allbox = res.data.data.data.allbox;\r\n        this.isprice = res.data.data.data.isprice;\r\n        this.isserve = res.data.data.data.isserve;\r\n        this.noprice = res.data.data.data.noprice;\r\n        this.noserve = res.data.data.data.noserve;\r\n        this.with1 = res.data.data.with;\r\n\r\n        this.ad_with = res.data.data.ad_with;\r\n        this.ad_sum = res.data.data.ad_sum;\r\n        this.ad_canWith = res.data.data.ad_canWith;\r\n\r\n        this.canWith = res.data.data.canWith;\r\n        this.position = res.data.data.position;\r\n        this.page.total = res.data.data.total;\r\n        this.userList = res.data.data.log;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: this.page.total,\r\n        });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/examine/withdrawals/userlit"}]}