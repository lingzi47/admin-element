\n    <modify-pwd ref=\"modifyPwd\"></modify-pwd>\n  </div>\n</template>\n\n<script>\nimport { getUserInfo } from '@/api/user'\nimport modifyPwd from \"@/components/modifyPwd.vue\"\nexport default {\n  name: \"personal\",\n  components: {\n    modifyPwd\n  },\n  data() {\n    return {\n      userInfo: {},\n      rules: {},\n    }\n  },\n  created() {\n    this.getUserInfo();\n  },\n  mounted() { },\n  methods: {\n    async getUserInfo() {\t// 获取用户信息\n      let res = await getUserInfo({ token: this.$store.getters.token });\n      if (res.status == 200) {\n        // console.log(JSON.parse(JSON.stringify(res.data)));\n        res.data.sex = res.data.sex + \"\";\n        this.userInfo = res.data\n      }\n    },\n\t\tupdatePwd(){\t//修改密码\n\t\t\tthis.$refs.modifyPwd.show({userId: this.userInfo.id});\n\t\t}\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.personal {\n  .el-form .el-radio-group {\n    pointer-events: none;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/authority/personal"}]}