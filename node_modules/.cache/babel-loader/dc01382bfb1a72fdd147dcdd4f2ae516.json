l=\"ruleForm.type\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px\"\r\n                @change=\"change1\"\r\n              >\r\n                <el-option label=\"公司\" value=\"10\"></el-option>\r\n                <el-option label=\"渠道商\" value=\"20\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <el-form-item label=\"点位到期时间\" prop=\"year\">\r\n              <el-date-picker\r\n                style=\"width: 200px\"\r\n                v-model=\"ruleForm.year\"\r\n                :disabled=\"isDisable\"\r\n                value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                type=\"datetime\"\r\n                placeholder=\"选择日期时间\"\r\n              >\r\n              </el-date-picker>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"2\">\r\n            <el-form-item class=\"xian1\">\r\n              <el-checkbox v-model=\"check\" @change=\"checked\"\r\n                >无期限</el-checkbox\r\n              >\r\n            </el-form-item></el-col\r\n          >\r\n          <el-col :span=\"24\">\r\n            <el-form-item\r\n              label=\"渠道商id:\"\r\n              prop=\"position_user\"\r\n              v-if=\"this.ruleForm.type == 20\"\r\n            >\r\n              <el-input\r\n                v-model=\"ruleForm.position_user\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入渠道商id\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"物业名称\">\r\n              <el-input\r\n                v-model=\"property\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入物业名称\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"有无其他条款\">\r\n              <el-select\r\n                v-model=\"exclude\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px\"\r\n              >\r\n                <el-option label=\"有\" value=\"有\"></el-option>\r\n                <el-option label=\"无\" value=\"无\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"是否房产准入\">\r\n              <el-select\r\n                v-model=\"house\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px\"\r\n              >\r\n                <el-option label=\"是\" value=\"是\"></el-option>\r\n                <el-option label=\"否\" value=\"否\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"是否医美准入\">\r\n              <el-select\r\n                v-model=\"medical\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px\"\r\n              >\r\n                <el-option label=\"是\" value=\"是\"></el-option>\r\n                <el-option label=\"否\" value=\"否\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"商圈\">\r\n              <el-input\r\n                v-model=\"shop\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入商圈\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"附近商圈\">\r\n              <el-input\r\n                v-model=\"shop_name\"\r\n                style=\"width: 180px\"\r\n                placeholder=\"请输入附近商圈\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <el-form-ite