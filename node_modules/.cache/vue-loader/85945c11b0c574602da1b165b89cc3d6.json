   :resizable=\"false\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <span>{{\r\n            (page.currentPage - 1) * page.pageSize + scope.$index + 1\r\n          }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"活动\" align=\"center\">\r\n        <template slot-scope=\"scope\"\r\n          ><span\r\n            >普通会员商城消费单笔满({{ scope.row.money }})元赠送MT黑卡会员({{\r\n              scope.row.days\r\n            }})日</span\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"活动时间\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span\r\n            >{{ scope.row.at_start_time }}---{{ scope.row.at_end_time }}</span\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"created_at\" label=\"提交时间\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"ex_uname\" label=\"审核人\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"ex_time\" label=\"审核时间\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"ex_status\" label=\"审核状态\" align=\"center\">\r\n      </el-table-column>\r\n    </page-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { paylist, paydel } from \"@/request/api\";\r\nimport { checkPermission } from \"@/utils/permissions\";\r\n\r\nimport pageTable from \"@/components/pageTable.vue\";\r\n\r\nexport default {\r\n  name: \"user\",\r\n  components: {\r\n    pageTable,\r\n  },\r\n  data() {\r\n    return {\r\n      userList: [], // 用户列表\r\n\r\n      page: {\r\n        //分页信息\r\n        currentPage: 1, //当前页\r\n        pageSize: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.getlist(); //获取用户列表\r\n  },\r\n  mounted() {},\r\n  computed: {},\r\n  methods: {\r\n    go() {\r\n      this.$router.back();\r\n    },\r\n\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getlist();\r\n    },\r\n\r\n    getlist() {\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n        type: 2,\r\n      };\r\n      paylist(params).then((res) => {\r\n        this.userList = res.data.data.data;\r\n        this.page.total = res.data.data.total;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: this.page.total,\r\n        });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}