pecial/goods/components","sourcesContent":["<template>\r\n  <div class=\"AddDialog\">\r\n    <el-dialog\r\n      :title=\"type == 1 ? '新增' : '编辑'\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"900px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form\r\n        :model=\"goodsForm\"\r\n        ref=\"goodsForm\"\r\n        :rules=\"rules\"\r\n        label-width=\"auto\"\r\n      >\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"名称\" prop=\"goods_name\">\r\n            <el-input\r\n              v-model=\"goodsForm.goods_name\"\r\n              placeholder=\"请输入名称\"\r\n              :disabled=\"type == 1 ? false : true\"\r\n            >\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"库存\" prop=\"have_num\">\r\n            <el-input\r\n              v-model=\"goodsForm.have_num\"\r\n              placeholder=\"请输入库存\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"24\">\r\n          <el-form-item\r\n            label=\"分类\"\r\n            prop=\"type_id\"\r\n            :rules=\"[\r\n              { required: true, message: '分类不能为空', trigger: 'blur' },\r\n            ]\"\r\n          >\r\n            <el-cascader\r\n              ref=\"cascader\"\r\n              v-model=\"goodsForm.type_id\"\r\n              :options=\"typelist\"\r\n              class=\"typewidth\"\r\n              :props=\"customProps\"\r\n              filterable\r\n              clearable\r\n              @change=\"handleChange\"\r\n            ></el-cascader>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"现价格\" prop=\"new_price\">\r\n            <el-input\r\n              v-model=\"goodsForm.new_price\"\r\n              placeholder=\"请输入现价格\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"原价格\" prop=\"old_price\">\r\n            <el-input\r\n              v-model=\"goodsForm.old_price\"\r\n              placeholder=\"请输入原价格\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"推广佣金\" prop=\"rebater\">\r\n            <el-input\r\n              v-model=\"goodsForm.rebater\"\r\n              placeholder=\"请输入推广佣金\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"总销售\" prop=\"sale_num\">\r\n            <el-input\r\n              v-model=\"goodsForm.sale_num\"\r\n              placeholder=\"请输入总销售\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"保障\" prop=\"guarantee\">\r\n            <el-input\r\n              v-model=\"goodsForm.guarantee\"\r\n              placeholder=\"请输入保障\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"标签\" prop=\"label\">\r\n            <el-input\r\n              v-model=\"goodsForm.label\"\r\n              placeholder=\"请输入标签\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"简介\" prop=\"brief\">\r\n            <el-input\r\n              v-model=\"goodsForm.brief\"\r\n              placeholder=\"请输入简介\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"封面图\" prop=\"goods_img\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://y4.wjw.cool/command/ossUpload?filename=file\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n            >\r\n              <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" /><i\r\n                v-else\r\n                class=\"el-icon-plus avatar-uploader-icon\"\r\n              ></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"商品图\">\r\n            <el-upload\r\n              action=\"http://y4.wjw.cool/command/ossUpload?filename=file\"\r\n              list-type=\"picture-card\"\r\n              class=\"upload\"\r\n              :on-success=\"banneradd\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n            >\r\n              <i class=\"el-icon-plus\"></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"商品详情\"\r\n            ><file-fu v-model=\"goodsForm.content\" @change=\"change\"></file-fu>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\" v-if=\"this.type == 1\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\" v-if=\"this.type == 2\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { shopGoodsadd, shoptype, shopGoodsedit } from \"@/api/shop\";\r\nimport { getRoles } from \"@/api/role\";\r\nimport fileFu from \"./fileFu.vue\";\r\n\r\nexport default {\r\n  name: \"AddDialog\",\r\n  components: {\r\n    fileFu,\r\n  },\r\n  data() {\r\n    return {\r\n      customProps: {\r\n        multiple: false, // 启用多选\r\n        emitPath: true, // 只返回该节点的值\r\n        value: \"id\", // 自定义当前数组的键名\r\n        label: \"type_name\",\r\n        children: \"children\",\r\n      },\r\n      imageUrl: \"\",\r\n      banners: [],\r\n      typelist: [],\r\n      rolesList: [], //角色列表\r\n      type: 1, //1新增，2编辑\r\n      dialogVisible: false,\r\n      phoneEditor: null,\r\n      goodsForm: {\r\n        goods_name: \"\",\r\n        have_num: \"\",\r\n        brief: \"\",\r\n        label: \"\",\r\n        guarantee: \"\",\r\n        sale_num: \"\",\r\n        rebater: \"\",\r\n        old_price: \"\",\r\n        new_price: \"\",\r\n        type_id: \"\",\r\n        content: \"\",\r\n        shop_type: 1,\r\n        goods_img: \"\",\r\n      },\r\n      rules: {\r\n        goods_name: [],\r\n        have_num: [],\r\n        brief: [],\r\n        label: [],\r\n        guarantee: [],\r\n        sale_num: [],\r\n        rebater: [],\r\n        old_price: [],\r\n        new_price: [],\r\n        type_id: [],\r\n        type_id: [],\r\n        type_id: [],\r\n        type_id: [],\r\n      },\r\n    };\r\n  },\r\n  created: function () {\r\n    // this.getRolesList(); //获取角色\r\n  },\r\n  mounted: function () {},\r\n  methods: {\r\n    handleChange(val) {\r\n      let nodesInfo = this.$refs[\"cascader\"].getCheckedNodes();\r\n      console.log(nodesInfo[0].value);\r\n      this.goodsForm.type_id = nodesInfo[0].value;\r\n    },\r\n    change(val) {\r\n      // console.log(val);\r\n    },\r\n    handleEdit() {\r\n      let oldProcedure = this.$refs[\"fileFu\"].getVal();\r\n      console.log(oldProcedure);\r\n    },\r\n    async getRolesList() {\r\n      let res = await getRoles();\r\n      if (res.status == 200) {\r\n        this.rolesList = res.data;\r\n      }\r\n    },\r\n\r\n    //分类选择\r\n    async gettypelist() {\r\n      let res = await shoptype(this.goodstype);\r\n      if (res.status == 200) {\r\n        this.typelist = res.data;\r\n      }\r\n    },\r\n    show(type, row) {\r\n      this.gettypelist();\r\n      this.type = type;\r\n      this.dialogVisible = true;\r\n      if (type == 1) {\r\n        // console.log(row);\r\n        this.goodsForm = {};\r\n      } else {\r\n        this.goodsForm = row;\r\n        // console.log(this.goodsForm);\r\n        this.imgurl = this.goodsForm.banners;\r\n        console.log(this.imgurl);\r\n        console.log(this.goodsForm.banners);\r\n        this.imageUrl = this.goodsForm.goods_img;\r\n      }\r\n    },\r\n\r\n    close() {\r\n      this.dialogVisible = false;\r\n      this.$nextTick(() => {\r\n        this.$refs.goodsForm.clearValidate(); //关闭清空校验规则\r\n      });\r\n    },\r\n\r\n    handleAvatarSuccess(res, file) {\r\n      console.log(file);\r\n      console.log(res);\r\n      let imgurl = res.data;\r\n      this.imageUrl = imgurl;\r\n      this.goodsForm.goods_img = imgurl;\r\n    },\r\n\r\n    banneradd(res, file) {\r\n      console.log(file);\r\n      console.log(res);\r\n      let imgurl = res.data;\r\n      this.banners.push(imgurl);\r\n      this.goodsForm.banners = this.banners.join(\",\");\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传图片大小不能超过 2MB!\");\r\n      }\r\n      return isLt2M;\r\n    },\r\n\r\n    submitForm() {\r\n      this.goodsForm.shop_type = 1;\r\n\r\n      this.$refs.goodsForm.validate(async (valid) => {\r\n        if (valid) {\r\n          if (this.type == 1) {\r\n            let res = await shopGoodsadd(this.goodsForm);\r\n\r\n            if (res.status == 200) {\r\n              this.$message.success(\"新增成功\");\r\n            }\r\n          } else if (this.type == 2) {\r\n            let res = await shopGoodsedit(this.goodsForm);\r\n            if (res.status == 200) {\r\n              this.$message.success(\"编辑成功\");\r\n            }\r\n          } else {\r\n          }\r\n          this.$parent.shopList();\r\n          this.close();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n\r\n.wangeditor {\r\n  .explain {\r\n    margin-bottom: 10px;\r\n\r\n    a {\r\n      color: #409eff;\r\n    }\r\n\r\n    a:hover {\r\n      color: #66b1ff;\r\n      text-decoration: underline;\r\n    }\r\n  }\r\n\r\n  /deep/ #websiteEditorElem {\r\n    .w-e-toolbar {\r\n      z-index: 11 !important;\r\n    }\r\n\r\n    .w-e-text-container {\r\n      z-index: 10 !important;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}