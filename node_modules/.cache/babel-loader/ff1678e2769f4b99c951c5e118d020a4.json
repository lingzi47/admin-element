IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,cAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,IAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,WAFA;AAGA;AACA,OAZA;AAcA,KAlBA;AAmBA,IAAA,aAnBA,yBAmBA,OAnBA,EAmBA;AACA,UAAA,OAAA,IAAA,QAAA,EAAA;AACA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,SAFA;AAGA,OALA,MAKA,IAAA,OAAA,IAAA,YAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA,OALA,MAKA,IAAA,OAAA,IAAA,WAAA,EAAA;AACA;AACA,YAAA,MAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAAA;AACA;AACA;AAnCA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"navbar\">\n    <div class=\"head-logo\">\n      <i style=\"padding-right: 5px\" class=\"el-icon-s-home\"></i>\n      预见App后台管理系统\n    </div>\n    <ul class=\"head-right\">\n      <li class=\"head-item\">\n        <i class=\"el-icon-date\" style=\"margin-right: 6px\"></i>\n        当前时间：{{ new Date().format(\"yyyy年MM月dd日 dddd\") }}\n      </li>\n      <li class=\"head-item\">\n        <el-dropdown @command=\"handleCommand\">\n          <div type=\"primary\">\n            <span class=\"user-name\">{{ name }}</span>\n            <i class=\"el-icon-arrow-down el-icon--right\"></i>\n          </div>\n          <el-dropdown-menu slot=\"dropdown\">\n            <!-- <el-dropdown-item command=\"userCenter\">\n              <i class=\"el-icon-user\"></i>个人信息\n            </el-dropdown-item>\n            <el-dropdown-item command=\"updatePwd\">\n              <i class=\"el-icon-edit\"></i>修改密码\n            </el-dropdown-item> -->\n            <!-- command=\"logout\" -->\n            <el-dropdown-item>\n              <p @click=\"loginout\">\n                <i class=\"el-icon-switch-button\"></i>退出登录\n              </p>\n            </el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </li>\n      <li style=\"line-height: initial\">\n        <!-- 修改密码 -->\n        <modify-pwd ref=\"modifyPwd\"></modify-pwd>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport { userOut } from \"@/request/api\";\nimport modifyPwd from \"@/components/modifyPwd.vue\";\nexport default {\n  name: \"Navbar\",\n  components: {\n    modifyPwd,\n  },\n  data() {\n    return {\n      name: \"\",\n    };\n  },\n  created() {\n    let name = sessionStorage.getItem(\"role_name\");\n    this.name = name;\n  },\n  computed: {\n    ...mapGetters([\"userInfo\"]),\n  },\n  methods: {\n    loginout() {\n      let uid = sessionStorage.getItem(\"uid\");\n      this.uid = uid;\n      userOut({ id: this.uid, token: sessionStorage.getItem(\"token\") }).then(\n        (res) => {\n          if (res.data.code == 200) {\n            this.$message({\n              message: \"退出成功\",\n              type: \"success\",\n            });\n            sessionStorage.clear();\n            this.$store.dispatch(\"user/logout\").then(() => {\n              window.location.reload();\n            });\n          }\n        }\n      );\n    },\n    handleCommand(command) {\n      if (command == \"logout\") {\n        //退出登录\n        this.$store.dispatch(\"user/logout\").then(() => {\n          window.location.reload();\n        });\n      } else if (command == \"userCenter\") {\n        //个人中心\n        this.$router.push({\n          name: \"personal\",\n        });\n      } else if (command == \"updatePwd\") {\n        //修改密码\n        let userId = this.$store.getters.userInfo.id;\n        this.$refs.modifyPwd.show({ userId: userId });\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.navbar {\n  padding: 0 10px;\n\n  .head-logo {\n    padding: 0 10px;\n    font-size: 24px;\n    font-weight: bold;\n  }\n\n  .head-right {\n    display: flex;\n\n    .head-item {\n      padding: 0 15px;\n      font-size: 15px;\n      color: #606266;\n      cursor: pointer;\n    }\n\n    .head-item:hover {\n      color: #409eff;\n    }\n\n    .user-tx {\n      width: 45px;\n      height: 45px;\n      border-radius: 6px;\n      margin-right: 5px;\n      vertical-align: middle;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/layout/components"}]}