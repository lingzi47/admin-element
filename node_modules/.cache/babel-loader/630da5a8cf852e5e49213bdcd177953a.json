IAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,WAAA,CAAA;AACA,UAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA;AADA,SAAA;AAGA,OANA;AAOA,KAzDA;AA0DA,IAAA,IA1DA,gBA0DA,GA1DA,EA0DA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,WAAA;AACA,KA9DA;AA+DA,IAAA,KA/DA,mBA+DA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,KAlEA;AAmEA,IAAA,MAnEA,oBAmEA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,QAAA,CAAA,QAAA,GAAA,EAAA;AACA,KAtEA;AAuEA,IAAA,UAvEA,wBAuEA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KADA;AAAA;AAAA;AAAA;;AAEA,kBAAA,MAFA,GAEA;AACA,oBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA,QADA;AAEA,oBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CAFA;AAGA,oBAAA,OAAA,EAAA,MAAA,CAAA;AAHA,mBAFA;AAOA,kBAAA,YAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,wBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,sBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,sBAAA,MAAA,CAAA,WAAA;;AACA,sBAAA,MAAA,CAAA,MAAA;AACA,qBAJA,MAIA;AACA,sBAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;;AACA,sBAAA,MAAA,CAAA,MAAA;;AACA,sBAAA,MAAA,CAAA,WAAA;AACA;AACA,mBAVA;AAPA;AAAA;;AAAA;AAAA,oDAmBA,KAnBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAsBA;AA9FA;AA5BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"editData\">\r\n    <el-dialog\r\n      title=\"查看\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"1200px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n    >\r\n      <el-form :inline=\"true\">\r\n        <el-form-item style=\"float: right\">\r\n          <el-button\r\n            v-if=\"checkPermission('usersearch')\"\r\n            type=\"primary\"\r\n            @click=\"addData\"\r\n            >添加</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n      <page-table\r\n        ref=\"dataTable\"\r\n        :data=\"userList\"\r\n        @changeCurrentPage=\"changeCurrent\"\r\n      >\r\n        <el-table-column label=\"序号\" align=\"center\" fixed :resizable=\"false\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{\r\n              (page.currentPage - 1) * page.pageSize + scope.$index + 1\r\n            }}</span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"goods_name\"\r\n          label=\"商品名称\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          :resizable=\"false\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-link\r\n              type=\"danger\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"deleteData(scope.row)\"\r\n              >删除</el-link\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </page-table>\r\n    </el-dialog>\r\n    <el-dialog\r\n      :title=\"type == 1 ? '新增' : '编辑'\"\r\n      :visible.sync=\"Visible\"\r\n      width=\"800px\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close1\"\r\n    >\r\n      <el-form\r\n        label-width=\"auto\"\r\n        :model=\"ruleForm\"\r\n        ref=\"ruleForm\"\r\n        :rules=\"rules\"\r\n      >\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"商品名称\" prop=\"goods_id\">\r\n              <el-select\r\n                v-model=\"ruleForm.goods_id\"\r\n                clearable\r\n                style=\"width: 230px\"\r\n                filterable\r\n                placeholder=\"请选择商品\"\r\n              >\r\n                <el-option\r\n                  v-for=\"item in arr\"\r\n                  :value=\"item.id\"\r\n                  :label=\"item.goods_name\"\r\n                  :key=\"item.id\"\r\n                ></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  plangoodslist,\r\n  plangoodsadd,\r\n  plangoodsdel,\r\n  xunnigoods,\r\n} from \"@/request/api\";\r\nimport { checkPermission } from \"@/utils/permissions\";\r\n\r\nimport pageTable from \"@/components/pageTable.vue\";\r\nexport default {\r\n  name: \"AddDialog\",\r\n  components: { pageTable },\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      type: \"\",\r\n      dialogVisible: false,\r\n      Visible: false,\r\n      ruleForm: { goods_id: \"\" },\r\n      teamid: \"\",\r\n      page: {\r\n        //分页信息\r\n        currentPage: 1, //当前页\r\n        pageSize: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n      arr: [],\r\n      userList: [], // 用户列表\r\n      rules: {\r\n        goods_id: [{ required: true, message: \"请选择商品\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getlist();\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    addData() {\r\n      this.type = 1;\r\n      this.Visible = true;\r\n    },\r\n    getlist() {\r\n      let params = {\r\n        token: sessionStorage.getItem(\"token\"),\r\n      };\r\n      xunnigoods(params).then((res) => {\r\n        this.arr = res.data.data;\r\n      });\r\n    },\r\n    checkPermission,\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.currentPage = page;\r\n      this.page.pageSize = size;\r\n      this.getUserList();\r\n    },\r\n    deleteData(row) {\r\n      this.$confirm(\"是否删除此信息？\", \"提示\", {\r\n        type: \"warning\",\r\n      })\r\n        .then(async () => {\r\n          let params = {\r\n            id: row.id,\r\n            token: sessionStorage.getItem(\"token\"),\r\n          };\r\n          plangoodsdel(params).then((res) => {\r\n            if (res.status == 200) {\r\n              this.getUserList();\r\n              this.$message.success(\"删除成功\");\r\n            } else {\r\n              this.$message.error(res.data.msg);\r\n              this.getUserList();\r\n            }\r\n          });\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    getUserList() {\r\n      let token = sessionStorage.getItem(\"token\");\r\n      this.token = token;\r\n      let params = {\r\n        page: this.page.currentPage,\r\n        limit: this.page.pageSize,\r\n        token: sessionStorage.getItem(\"token\"),\r\n        plan_id: this.teamid,\r\n      };\r\n      plangoodslist(params).then((res) => {\r\n        this.page.total = res.data.count;\r\n        this.userList = res.data.data;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: this.page.total,\r\n        });\r\n      });\r\n    },\r\n    show(row) {\r\n      this.dialogVisible = true;\r\n      this.teamid = row.id;\r\n      this.getUserList();\r\n    },\r\n    close() {\r\n      this.dialogVisible = false;\r\n      this.teamid = \"\";\r\n    },\r\n    close1() {\r\n      this.Visible = false;\r\n      this.ruleForm.goods_id = \"\";\r\n    },\r\n    submitForm() {\r\n      this.$refs.ruleForm.validate(async (valid) => {\r\n        if (valid) {\r\n          let params = {\r\n            goods_id: this.ruleForm.goods_id,\r\n            token: sessionStorage.getItem(\"token\"),\r\n            plan_id: this.teamid,\r\n          };\r\n          plangoodsadd(params).then((res) => {\r\n            if (res.data.code == 200) {\r\n              this.$message.success(\"新增成功\");\r\n              this.getUserList();\r\n              this.close1();\r\n            } else {\r\n              this.$message(res.data.msg);\r\n              this.close1();\r\n              this.getUserList();\r\n            }\r\n          });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/dingdan/dingdanz/components"}]}