;AAAA;;AAAA;AA6BA,yBAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AA7BA;AAAA,yBA8BA,cAAA,CAAA;AACA,oBAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,IADA;AAEA,oBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAFA;AAGA,oBAAA,GAAA,EAAA,MAAA,CAAA,EAHA;AAIA,oBAAA,SAAA,EAAA,CAJA;AAKA,oBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KALA;AAMA,oBAAA,KAAA,EAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CANA;AAOA,oBAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAPA,mBAAA,CA9BA;;AAAA;AA8BA,kBAAA,KA9BA;AAuCA,sBAAA,KAAA,CAAA,MAAA,IAAA,GAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,KAAA,CAAA,GAAA;;AAvCA;AAyCA,kBAAA,MAAA,CAAA,OAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,KAAA;;AA1CA;AAAA;;AAAA;AAAA,mDA4CA,KA5CA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AA+CA;AA/EA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"roleAddOrEditDialog\">\n    <el-dialog\n      :title=\"type == 1 ? '新增' : '编辑'\"\n      :visible.sync=\"dialogVisible\"\n      width=\"400px\"\n      :close-on-click-modal=\"false\"\n      @close=\"close\"\n    >\n      <el-form\n        :model=\"ruleForm\"\n        ref=\"ruleForm\"\n        :rules=\"rules\"\n        label-width=\"auto\"\n      >\n        <el-input type=\"hidden\" v-model=\"ruleForm.type_id\"></el-input>\n        <el-row :gutter=\"20\">\n          <el-col :span=\"24\">\n            <el-form-item label=\"分类名称\" prop=\"name\">\n              <el-input v-model=\"ruleForm.name\" placeholder=\"请输入分类\">\n              </el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"24\">\n            <el-form-item label=\"排序(大->小)\" prop=\"title\">\n              <el-input\n                v-model=\"ruleForm.weigh\"\n                placeholder=\"排序(大->小)\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"24\">\n            <el-form-item label=\"分类简介\" prop=\"description\">\n              <el-input\n                v-model=\"ruleForm.brief\"\n                placeholder=\"请输入简介\"\n              ></el-input>\n            </el-form-item>\n          </el-col>\n\n          <el-col :span=\"24\">\n            <el-form-item label=\"图标\" prop=\"description\">\n              <el-upload\n                class=\"avatar-uploader\"\n                action=\"https://yujian02.xyz/command/ossUpload?filename=file\"\n                :show-file-list=\"false\"\n                :on-success=\"handleAvatarSuccess\"\n                :before-upload=\"beforeAvatarUpload\"\n              >\n                <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\n                <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n              </el-upload>\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { shopTypeadd, shopTypeedit, shopTyptreeadd } from \"@/api/shop\";\nexport default {\n  name: \"roleAddOrEditDialog\",\n  components: {},\n  data() {\n    return {\n      id: \"\",\n      imageUrl: \"\",\n      type: 1, //1新增，2编辑，3分类新增\n      dialogVisible: false,\n      ruleForm: [],\n      rules: {\n        name: [\n          { required: true, message: \"分类名称不能为空\", trigger: \"blur\" },\n        ],\n      },\n    };\n  },\n  created: function () {},\n  mounted: function () {},\n  methods: {\n    show(type, row) {\n      this.id = row.id;\n      this.type = type;\n      this.dialogVisible = true;\n      this.ruleForm = row;\n      this.imageUrl = this.ruleForm.icon;\n      if (this.type == 3) {\n        this.ruleForm = {};\n        this.imageUrl = \"\";\n      }\n    },\n    close() {\n      this.dialogVisible = false;\n      this.$nextTick(() => {\n        this.$refs.ruleForm.clearValidate(); //关闭清空校验规则\n      });\n    },\n    handleAvatarSuccess(res, file) {\n      let imgurl = res.data;\n      this.imageUrl = imgurl;\n      this.ruleForm.icon = imgurl;\n    },\n    beforeAvatarUpload(file) {\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isLt2M) {\n        this.$message.error(\"上传图片大小不能超过 2MB!\");\n      }\n      return isLt2M;\n    },\n\n    submitForm() {\n      this.$refs.ruleForm.validate(async (valid) => {\n        this.ruleForm.shop_type = 3;\n        delete this.ruleForm[\"create_time\"];\n        delete this.ruleForm[\"update_time\"];\n        delete this.ruleForm[\"delete_time\"];\n        if (valid) {\n          if (this.type == 1) {\n            let res = await shopTypeadd({\n              name: this.ruleForm.name,\n              shop_type: 3,\n              weigh: this.ruleForm.weigh,\n              brief: this.ruleForm.brief,\n              token: sessionStorage.getItem(\"token\"),\n              icon: this.ruleForm.icon,\n            });\n            if (res.status == 200) this.$message.success(res.msg);\n            else this.$message.warning(res.message);\n          } else if (this.type == 2) {\n            let res = await shopTypeedit({\n              name: this.ruleForm.name,\n              weigh: this.ruleForm.weigh,\n              id: this.id,\n              shop_type: 3,\n              brief: this.ruleForm.brief,\n              token: sessionStorage.getItem(\"token\"),\n              icon: this.ruleForm.icon,\n            });\n            if (res.status == 200) this.$message.success(res.msg);\n          } else {\n            delete this.ruleForm[\"id\"];\n            let res = await shopTyptreeadd({\n              name: this.ruleForm.name,\n              weigh: this.ruleForm.weigh,\n              pid: this.id,\n              shop_type: 3,\n              brief: this.ruleForm.brief,\n              token: sessionStorage.getItem(\"token\"),\n              icon: this.ruleForm.icon,\n            });\n            if (res.status == 200) this.$message.success(res.msg);\n          }\n          this.$parent.getlist();\n          this.close();\n        } else {\n          return false;\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style>\n.avatar-uploader .el-upload {\n  border: 1px dashed #d9d9d9;\n  border-radius: 6px;\n  cursor: pointer;\n  position: relative;\n  overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409eff;\n}\n.avatar-uploader-icon {\n  font-size: 28px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 178px;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n}\n</style>\n"],"sourceRoot":"src/views/shop/life/lifetype"}]}