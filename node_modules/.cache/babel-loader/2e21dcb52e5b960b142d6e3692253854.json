;AACA,WAAA,MAAA,GAAA,GAAA,CAAA,EAAA;AACA,KA5DA;AA6DA,IAAA,OA7DA,mBA6DA,GA7DA,EA6DA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AAEA,WAAA,cAAA,GAAA,IAAA;AACA,KAjEA;AAkEA;AACA,IAAA,MAnEA,oBAmEA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KArEA;AAsEA,IAAA,UAtEA,sBAsEA,IAtEA,EAsEA,GAtEA,EAsEA;AAAA;;AACA,UAAA,IAAA,IAAA,CAAA,IAAA,KAAA,eAAA,CAAA,MAAA,IAAA,CAAA,IAAA,IAAA,IAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,UAAA,EAAA,IAAA,EAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA,EAGA,IAHA,uEAGA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACA,UAAA,CAAA;AACA,oBAAA,EAAA,EAAA,GAAA,CAAA;AADA,mBAAA,CADA;;AAAA;AACA,kBAAA,GADA;;AAIA,sBAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,MAAA;;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAHA,IAYA,KAZA,CAYA,YAAA,CAAA,CAZA;AAaA,OAdA,MAcA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,WAAA;AACA;AACA;AAxFA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"AddDialog\">\r\n    <el-dialog title=\"SKU规格\" :visible.sync=\"dialogVisible\" width=\"900px\">\r\n      <div class=\"addsku\">\r\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"addsku()\"\r\n          >新增</el-button\r\n        >\r\n      </div>\r\n      <div class=\"user\">\r\n        <page-table\r\n          ref=\"dataTable\"\r\n          :data=\"skuList\"\r\n          @changeCurrentPage=\"changeCurrent\"\r\n        >\r\n          <el-table-column\r\n            type=\"index\"\r\n            fixed\r\n            width=\"40\"\r\n            :resizable=\"false\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            prop=\"name\"\r\n            fixed\r\n            label=\"商品规格\"\r\n            width=\"240\"\r\n            :resizable=\"false\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            prop=\"skuname\"\r\n            fixed\r\n            label=\"商品属性\"\r\n            :resizable=\"false\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            fixed=\"right\"\r\n            label=\"操作\"\r\n            width=\"160\"\r\n            :resizable=\"false\"\r\n          >\r\n            <template slot-scope=\"scope\">\r\n              <el-link type=\"primary\" @click=\"editsku(scope.row)\">编辑</el-link>\r\n              <el-link\r\n                type=\"danger\"\r\n                style=\"margin-left: 10px\"\r\n                @click=\"deleteData(2, scope.row)\"\r\n                >删除</el-link\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </page-table>\r\n        <!-- 新增编辑弹窗 -->\r\n        <!-- <add-sku ref=\"addskuData\" /> -->\r\n      </div>\r\n    </el-dialog>\r\n    <div id=\"\">\r\n      <el-dialog\r\n        title=\"SKU规格新增\"\r\n        :visible.sync=\"dialogVisible1\"\r\n        width=\"400px\"\r\n      >\r\n        <el-form\r\n          :model=\"skulist\"\r\n          ref=\"skulist\"\r\n          label-width=\"100px\"\r\n          class=\"demo-dynamic\"\r\n        >\r\n          <el-form-item prop=\"name\" label=\"商品规格\">\r\n            <el-input v-model=\"skulist.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            v-for=\"(domain, index) in skulist.domains\"\r\n            :label=\"'属性' + index\"\r\n            :key=\"domain.key\"\r\n            :prop=\"'domains.' + index + '.value'\"\r\n          >\r\n            <el-input v-model=\"domain.value\"></el-input\r\n            ><el-button @click.prevent=\"removeDomain(domain)\">删除</el-button>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"submitForm()\">提交</el-button>\r\n            <el-button @click=\"addDomain\">新增属性</el-button>\r\n            <el-button @click=\"resetForm('skulist')\">重置</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { shopskuindex, shopskuadd, shopskudel } from \"@/api/shop\";\r\nimport pageTable from \"@/components/pageTable.vue\";\r\n// import addSku from \"./addOrEditSku.vue\";\r\n\r\nexport default {\r\n  name: \"SkuIndex\",\r\n  components: {\r\n    pageTable,\r\n    // addSku,\r\n  },\r\n  data() {\r\n    return {\r\n      skulist: {\r\n        domains: [\r\n          {\r\n            value: \"\",\r\n          },\r\n        ],\r\n        name: \"\",\r\n      },\r\n\r\n      form: {},\r\n      goodid: \"\",\r\n      skuList: [], //列表\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      page: {\r\n        //分页信息\r\n        page: 1, //当前页\r\n        limit: 10, //每页条数\r\n        total: 0, //总条数\r\n      },\r\n    };\r\n  },\r\n  created: function () {},\r\n  mounted: function () {},\r\n  methods: {\r\n    // 切换分页\r\n    changeCurrent(page, size) {\r\n      this.page.page = page;\r\n      this.page.limit = size;\r\n      this.shopsku(this.goodid);\r\n    },\r\n\r\n    //SKU列表\r\n    async shopsku(goodid) {\r\n      this.form.goods_id = goodid;\r\n      let res = await shopskuindex(Object.assign(this.page, this.form));\r\n      if (res.status == 200) {\r\n        this.page.total = res.count;\r\n        this.skuList = res.data;\r\n        this.$refs.dataTable.setPageInfo({\r\n          total: res.count,\r\n        });\r\n      }\r\n    },\r\n\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    removeDomain(item) {\r\n      var index = this.skulist.domains.indexOf(item);\r\n      if (index !== -1) {\r\n        this.skulist.domains.splice(index, 1);\r\n      }\r\n    },\r\n    addDomain() {\r\n      this.skulist.domains.push({\r\n        value: \"\",\r\n        key: Date.now(),\r\n      });\r\n    },\r\n\r\n    submitForm() {\r\n      console.log(this.skulist.domains);\r\n      this.$refs.skulist.validate(async (valid) => {\r\n        if (valid) {\r\n          this.skulist.goods_id = this.goodid;\r\n          let res = await shopskuadd(this.skulist);\r\n          if (res.status == 200) {\r\n            this.$message.success(\"新增成功\");\r\n          }\r\n          this.dialogVisible1 = false;\r\n          this.shopsku(this.goodid);\r\n          this.close();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n\r\n    show(type, row) {\r\n      this.type = type;\r\n      this.dialogVisible = true;\r\n      this.shopsku(row.id);\r\n      this.goodid = row.id;\r\n    },\r\n    editsku(row) {\r\n      console.log(row);\r\n\r\n      this.dialogVisible1 = true;\r\n    },\r\n    //sku新增\r\n    addsku() {\r\n      this.dialogVisible1 = true;\r\n    },\r\n    deleteData(type, row) {\r\n      if ((type == 1 && this.tableSelectList.length == 1) || type == 2) {\r\n        this.$confirm(\"是否删除此规格？\", \"提示\", {\r\n          type: \"warning\",\r\n        })\r\n          .then(async () => {\r\n            let res = await shopskudel({\r\n              id: row.id,\r\n            });\r\n            if (res.status == 200) {\r\n              this.shopsku(this.goodid);\r\n              this.$message.success(\"删除成功\");\r\n            }\r\n          })\r\n          .catch(() => {});\r\n      } else {\r\n        this.$message.warning(\"请选择一条数据删除\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.wangeditor {\r\n  .explain {\r\n    margin-bottom: 10px;\r\n\r\n    a {\r\n      color: #409eff;\r\n    }\r\n\r\n    a:hover {\r\n      color: #66b1ff;\r\n      text-decoration: underline;\r\n    }\r\n  }\r\n\r\n  /deep/ #websiteEditorElem {\r\n    .w-e-toolbar {\r\n      z-index: 11 !important;\r\n    }\r\n    .w-e-text-container {\r\n      z-index: 10 !important;\r\n    }\r\n  }\r\n}\r\n\r\n.addsku {\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/views/shop/special/goods/components"}]}