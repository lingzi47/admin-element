ue\" :model=\"form\">\n      <el-form-item>\n        <el-cascader v-model=\"form.value1\" :options=\"areaArr\" :props=\"{value: 'code', label: 'name'}\" placeholder=\"带搜索的级联\" filterable @change=\"change\"></el-cascader>\n      </el-form-item>\n      <el-form-item>\n        <el-cascader v-model=\"form.value2\" :options=\"areaArr\" :props=\"{value: 'code', label: 'name'}\" placeholder=\"自定义节点\">\n          <template slot-scope=\"{ node, data }\">\n            <span>{{ data.name }}</span>\n            <span> ({{ data.code }}) </span>\n          </template>\n        </el-cascader>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport { areaListData } from \"@/utils/area\";\nexport default {\n  name: \"CascaderAssembly\",\n  data() {\n    return {\n      areaArr: [],\n      form: {\n        value1: '',\n        value2: '',\n        value3: '',\n      },\n    }\n  },\n  created() {\n    this.setData(areaListData());\n    this.areaArr = areaListData();\n  },\n  mounted() { },\n  methods: {\n    setData(data) {\n      data.map(item => {\n        item[\"value\"] = item.code;\n        item[\"label\"] = item.name;\n        if (item.children) {\n          this.setData(item.children);\n        }\n      })\n    },\n    change(data) {\n      this.$message('选中：' + JSON.stringify(data));\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"]}]}